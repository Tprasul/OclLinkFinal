          "_sourceTheme": "startm5",
          "_customHTML": "<!DOCTYPE html>\r\n<html lang=\"ru\">\r\n<head>\r\n  <meta charset=\"UTF-8\">\r\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n  <title>–û–±—Ä–∞—Ç–Ω–∞—è —Å–≤—è–∑—å</title>\r\n  \r\n  <!-- –°—Ç–∏–ª–∏ -->\r\n  <style>\r\n    /* –ì–ª–æ–±–∞–ª—å–Ω—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –¥–ª—è –æ—Ç–∑—ã–≤—á–∏–≤–æ–≥–æ –¥–∏–∑–∞–π–Ω–∞ */\r\n    *, *::before, *::after {\r\n      box-sizing: border-box;\r\n    }\r\n\r\n    body {\r\n      background-color: #f8f8f8;\r\n      display: flex;\r\n      flex-direction: column; /* –≠–ª–µ–º–µ–Ω—Ç—ã –≤ —Å—Ç–æ–ª–±–∏–∫, –¥–ª—è —Ü–µ–Ω—Ç—Ä–∏—Ä–æ–≤–∞–Ω–∏—è */\r\n      justify-content: center;\r\n      align-items: center;\r\n      min-height: 100vh;\r\n      margin: 0;\r\n      padding: 20px; /* –û—Ç—Å—Ç—É–ø –ø–æ –∫—Ä–∞—è–º –¥–ª—è –∞–¥–∞–ø—Ç–∏–≤–Ω–æ—Å—Ç–∏ */\r\n      font-family: sans-serif;\r\n    }\r\n    .form-container {\r\n      max-width: 600px;\r\n      width: 100%; /* –ó–∞–Ω–∏–º–∞–µ—Ç –≤—Å—é –¥–æ—Å—Ç—É–ø–Ω—É—é —à–∏—Ä–∏–Ω—É –¥–æ max-width */\r\n      padding: 30px;\r\n      border: 1px solid #ddd;\r\n      border-radius: 12px;\r\n      background-color: #FFE4E1; /* –°–≤–µ—Ç–ª–æ-—Ä–æ–∑–æ–≤—ã–π */\r\n      box-shadow: 0 8px 20px rgba(0,0,0,0.1);\r\n    }\r\n    .avatar-img {\r\n      width: 150px;\r\n      height: 150px;\r\n      border-radius: 50%;\r\n      object-fit: cover;\r\n      margin: 0 auto 20px auto; /* –¶–µ–Ω—Ç—Ä–∏—Ä–æ–≤–∞–Ω–∏–µ */\r\n      display: block;\r\n      border: 3px solid #d32f2f; /* –¶–≤–µ—Ç –∫–∞–∫ —É –∫–Ω–æ–ø–∫–∏ */\r\n      box-shadow: 0 0 15px rgba(0,0,0,0.15);\r\n    }\r\n    .form-title {\r\n      font-size: 28px;\r\n      font-weight: bold;\r\n      text-align: center;\r\n      margin-bottom: 30px;\r\n      color: #333;\r\n    }\r\n    .question-frame { /* –†–∞–º–∫–∞ –¥–ª—è –≤–æ–ø—Ä–æ—Å–æ–≤ –∏ –ø–æ–ª–µ–π –≤–≤–æ–¥–∞ */\r\n      border: 1px solid #ffcccb; \r\n      border-radius: 8px;\r\n      padding: 15px;\r\n      margin-bottom: 25px; \r\n      background-color: #fffafa; \r\n      box-shadow: 0 2px 5px rgba(0,0,0,0.05);\r\n    }\r\n    .question-frame label { /* –°—Ç–∏–ª—å –¥–ª—è —Ç–µ–∫—Å—Ç–∞ –≤–æ–ø—Ä–æ—Å–∞/–ø–æ–ª—è –≤–Ω—É—Ç—Ä–∏ —Ä–∞–º–∫–∏ */\r\n        margin-top: 0 !important; \r\n        margin-bottom: 12px !important; \r\n        display: block !important; \r\n        font-weight: bold !important; \r\n        color: #555 !important; \r\n        line-height: 1.4 !important; \r\n        word-wrap: break-word; \r\n        overflow-wrap: break-word; \r\n    }\r\n    label { /* –°–±—Ä–æ—Å –æ–±—â–∏—Ö —Å—Ç–∏–ª–µ–π label, —á—Ç–æ–±—ã –æ–Ω–∏ –Ω–µ –≤–ª–∏—è–ª–∏ –Ω–∞ —ç–ª–µ–º–µ–Ω—Ç—ã –≤–Ω—É—Ç—Ä–∏ —Ä–∞–º–æ–∫ */\r\n        font-weight: normal; \r\n        display: inline; \r\n        margin: 0; \r\n        padding: 0; \r\n        color: inherit;\r\n        line-height: normal;\r\n    }\r\n\r\n    input[type=\"text\"], textarea {\r\n      width: 100%; \r\n      padding: 12px;\r\n      border-radius: 8px;\r\n      border: 1px solid #ccc;\r\n      font-size: 16px;\r\n      margin-bottom: 0; \r\n    }\r\n    textarea {\r\n      resize: vertical; \r\n      min-height: 80px;\r\n    }\r\n    .radio-group {\r\n      display: flex; \r\n      flex-direction: column; /* –í–∞—Ä–∏–∞–Ω—Ç—ã –≤ —Å—Ç–æ–ª–±–∏–∫ –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é */\r\n      gap: 10px; \r\n      margin-bottom: 0; \r\n      flex-wrap: wrap; \r\n      justify-content: flex-start; \r\n    }\r\n    .radio-option {\r\n      display: flex;\r\n      align-items: center;\r\n      gap: 12px;\r\n      cursor: pointer;\r\n      padding: 10px;\r\n      border-radius: 8px;\r\n      background-color: transparent; \r\n      transition: background-color 0.2s ease;\r\n      flex-shrink: 0; \r\n    }\r\n    .radio-option:hover {\r\n      background-color: #f0f0f0; \r\n    }\r\n    .radio-option input[type=\"radio\"] {\r\n      display: none; \r\n    }\r\n    .custom-radio {\r\n      flex-shrink: 0; \r\n      width: 35px; \r\n      height: 35px;\r\n      background-color: #eee;\r\n      border-radius: 50%;\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: center;\r\n      font-weight: bold;\r\n      color: #333;\r\n      border: 2px solid transparent;\r\n      transition: 0.2s;\r\n      font-size: 18px;\r\n    }\r\n    /* –¶–≤–µ—Ç –∞–∫—Ç–∏–≤–Ω–æ–π –∫–Ω–æ–ø–∫–∏ –¥–ª—è –û–±—Ä–∞—Ç–Ω–æ–π —Å–≤—è–∑–∏ */\r\n    .radio-option input[type=\"radio\"]:checked + .custom-radio {\r\n      border-color: #d32f2f; /* –ö—Ä–∞—Å–Ω—ã–π */\r\n      background-color: #d32f2f; /* –ö—Ä–∞—Å–Ω—ã–π */\r\n      color: white;\r\n      box-shadow: 0 0 10px rgba(211, 47, 47, 0.5); /* –°–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤—É—é—â–∞—è —Ç–µ–Ω—å */\r\n    }\r\n    .submit-btn {\r\n      margin-top: 40px;\r\n      padding: 15px 25px;\r\n      background-color: #d32f2f;\r\n      color: white;\r\n      font-size: 18px;\r\n      font-weight: bold;\r\n      border: none;\r\n      border-radius: 10px;\r\n      cursor: pointer;\r\n      width: 100%;\r\n      transition: background-color 0.3s ease, transform 0.2s ease;\r\n      box-shadow: 0 4px 10px rgba(0,0,0,0.3);\r\n    }\r\n    .submit-btn:hover:not(:disabled) {\r\n      background-color: #b71c1c;\r\n      transform: translateY(-2px);\r\n    }\r\n    .submit-btn:disabled {\r\n      background-color: #ff7f7f; \r\n      cursor: not-allowed;\r\n      box-shadow: none;\r\n    }\r\n    /* –°—Ç–∏–ª—å –¥–ª—è —Å–µ–∫—Ü–∏–æ–Ω–Ω—ã—Ö –∑–∞–≥–æ–ª–æ–≤–∫–æ–≤ */\r\n    .section-title {\r\n        background: #d32f2f; /* –ö—Ä–∞—Å–Ω—ã–π —Ñ–æ–Ω */\r\n        color: white;\r\n        padding: 10px;\r\n        margin-top: 30px;\r\n        margin-bottom: 20px; /* –î–æ–±–∞–≤–∏–ª –æ—Ç—Å—Ç—É–ø —Å–Ω–∏–∑—É */\r\n        border-radius: 8px;\r\n        font-weight: bold;\r\n        text-align: center;\r\n    }\r\n\r\n    /* –ú–µ–¥–∏–∞-–∑–∞–ø—Ä–æ—Å—ã –¥–ª—è –∞–¥–∞–ø—Ç–∞—Ü–∏–∏ –ø–æ–¥ –º–æ–±–∏–ª—å–Ω—ã–µ */\r\n    @media (max-width: 768px) {\r\n        body { padding: 15px; }\r\n        .form-container { padding: 20px; }\r\n        .form-title { font-size: 24px; margin-bottom: 20px; }\r\n        .question-frame { padding: 10px; margin-bottom: 20px; }\r\n        .question-frame label { font-size: 15px; }\r\n        input[type=\"text\"], textarea { \r\n            font-size: 15px;\r\n            padding: 10px;\r\n        }\r\n        .radio-group {\r\n            flex-direction: column; /* –í–∞—Ä–∏–∞–Ω—Ç—ã –≤ —Å—Ç–æ–ª–±–∏–∫ –Ω–∞ –º–æ–±–∏–ª—å–Ω—ã—Ö */\r\n            gap: 8px; \r\n        }\r\n        .radio-option {\r\n            padding: 8px;\r\n        }\r\n        .custom-radio {\r\n            width: 30px; \r\n            height: 30px;\r\n            font-size: 16px;\r\n        }\r\n        .submit-btn {\r\n            font-size: 16px;\r\n            padding: 12px 20px;\r\n            margin-top: 30px;\r\n        }\r\n    }\r\n\r\n    @media (max-width: 480px) { /* –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –∞–¥–∞–ø—Ç–∞—Ü–∏—è –¥–ª—è –æ—á–µ–Ω—å –º–∞–ª–µ–Ω—å–∫–∏—Ö —ç–∫—Ä–∞–Ω–æ–≤ */\r\n        body { padding: 10px; }\r\n        .form-container { padding: 15px; }\r\n        .form-title { font-size: 22px; margin-bottom: 15px; }\r\n        .avatar-img { width: 120px; height: 120px; }\r\n        .question-frame { margin-bottom: 15px; }\r\n    }\r\n  </style>\r\n  \r\n  <!-- SweetAlert2 –¥–ª—è –∫—Ä–∞—Å–∏–≤—ã—Ö –≤—Å–ø–ª—ã–≤–∞—é—â–∏—Ö –æ–∫–æ–Ω -->\r\n  <script src=\"https://cdn.jsdelivr.net/npm/sweetalert2@11\"></script>\r\n</head>\r\n<body>\r\n\r\n  <div class=\"form-container\">\r\n    <img src=\"https://i.imgur.com/SxzkAWe.jpeg\" alt=\"–õ–æ–≥–æ—Ç–∏–ø\" class=\"avatar-img\">\r\n    <div class=\"form-title\">–û–±—Ä–∞—Ç–Ω–∞—è —Å–≤—è–∑—å</div>\r\n    <form id=\"feedbackForm\">\r\n      <!-- –°–∫—Ä—ã—Ç–æ–µ –ø–æ–ª–µ –¥–ª—è –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ü–∏–∏ –æ–ø—Ä–æ—Å–∞ -->\r\n      <input type=\"hidden\" name=\"surveyId\" value=\"feedback_survey\"> \r\n\r\n      <div class=\"question-frame\">\r\n        <label for=\"tvHost\">–ö—Ç–æ –ø—Ä–æ–≤–µ–ª Table visit?</label>\r\n        <input type=\"text\" id=\"tvHost\" name=\"tvHost\" required placeholder=\"–í–∞—à–µ –∏–º—è‚Ä¶\">\r\n      </div>\r\n\r\n      <!-- 1. –ö–∞–∫–æ–µ –≤–∞—à–µ –ø–µ—Ä–≤–æ–µ –≤–ø–µ—á–∞—Ç–ª–µ–Ω–∏–µ –æ –Ω–∞—à–µ–º —Ä–µ—Å—Ç–æ—Ä–∞–Ω–µ? -->\r\n      <div class=\"question-frame\">\r\n        <label>1. –ö–∞–∫–æ–µ –≤–∞—à–µ –ø–µ—Ä–≤–æ–µ –≤–ø–µ—á–∞—Ç–ª–µ–Ω–∏–µ –æ –Ω–∞—à–µ–º —Ä–µ—Å—Ç–æ—Ä–∞–Ω–µ?</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q1_first_impression\" value=\"–í—Å–µ –æ—á–µ–Ω—å —á–∏—Å—Ç–æ, –º–Ω–µ –≤—Å–µ –Ω—Ä–∞–≤–∏—Ç—Å—è\" required><div class=\"custom-radio\">1</div>–í—Å–µ –æ—á–µ–Ω—å —á–∏—Å—Ç–æ, –º–Ω–µ –≤—Å–µ –Ω—Ä–∞–≤–∏—Ç—Å—è</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q1_first_impression\" value=\"–°—Ç–æ–ª—ã –±—ã–ª–∏ –Ω–µ —É–±—Ä–∞–Ω—ã, –≤ —Ü–µ–ª–æ–º –Ω–µ —Ç–∞–∫ –≤—Å–µ –ø–ª–æ—Ö–æ –∫–∞–∫ –º–æ–≥–ª–æ –±—ã—Ç—å\"><div class=\"custom-radio\">2</div>–°—Ç–æ–ª—ã –±—ã–ª–∏ –Ω–µ —É–±—Ä–∞–Ω—ã, –≤ —Ü–µ–ª–æ–º –Ω–µ —Ç–∞–∫ –≤—Å–µ –ø–ª–æ—Ö–æ –∫–∞–∫ –º–æ–≥–ª–æ –±—ã—Ç—å</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q1_first_impression\" value=\"–í—Å–µ –æ—á–µ–Ω—å –ø–ª–æ—Ö–æ, –±–æ–ª—å—à–µ —Å—é–¥–∞ –Ω–µ –ø—Ä–∏–¥—É\"><div class=\"custom-radio\">3</div>–í—Å–µ –æ—á–µ–Ω—å –ø–ª–æ—Ö–æ, –±–æ–ª—å—à–µ —Å—é–¥–∞ –Ω–µ –ø—Ä–∏–¥—É</label>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- 2. –ö–∞–∫ –≤—ã –æ—Ü–µ–Ω–∏—Ç–µ —Å–µ—Ä–≤–∏—Å –∏ —Ä–∞–±–æ—Ç—É –ø–µ—Ä—Å–æ–Ω–∞–ª–∞? -->\r\n      <div class=\"question-frame\">\r\n        <label>2. –ö–∞–∫ –≤—ã –æ—Ü–µ–Ω–∏—Ç–µ —Å–µ—Ä–≤–∏—Å –∏ —Ä–∞–±–æ—Ç—É –ø–µ—Ä—Å–æ–Ω–∞–ª–∞?</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q2_service_staff\" value=\"–ü—Ä–∏–≤–µ—Ç–ª–∏–≤—ã, –¥—Ä—É–∂–µ–ª—é–±–Ω—ã, –≤–Ω–∏–º–∞—Ç–µ–ª—å–Ω—ã –∏ –æ—Ç–∑—ã–≤—á–∏–≤—ã\" required><div class=\"custom-radio\">1</div>–ü—Ä–∏–≤–µ—Ç–ª–∏–≤—ã, –¥—Ä—É–∂–µ–ª—é–±–Ω—ã, –≤–Ω–∏–º–∞—Ç–µ–ª—å–Ω—ã –∏ –æ—Ç–∑—ã–≤—á–∏–≤—ã</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q2_service_staff\" value=\"–ù–∏—á–µ–≥–æ –ø—Ä–∏–º–µ—á–∞—Ç–µ–ª—å–Ω–æ–≥–æ\"><div class=\"custom-radio\">2</div>–ù–∏—á–µ–≥–æ –ø—Ä–∏–º–µ—á–∞—Ç–µ–ª—å–Ω–æ–≥–æ</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q2_service_staff\" value=\"–°–µ—Ä–≤–∏—Å –Ω–µ –ø–æ–Ω—Ä–∞–≤–∏–ª—Å—è!\"><div class=\"custom-radio\">3</div>–°–µ—Ä–≤–∏—Å –Ω–µ –ø–æ–Ω—Ä–∞–≤–∏–ª—Å—è!</label>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- 3. –ö–∞–∫ –≤—ã –æ—Ü–µ–Ω–∏—Ç–µ —Å–∫–æ—Ä–æ—Å—Ç—å –æ–±—Å–ª—É–∂–∏–≤–∞–Ω–∏—è? –ë—ã–ª–∏ –ª–∏ —É –≤–∞—Å –∫–∞–∫–∏–µ-–ª–∏–±–æ —Ç—Ä—É–¥–Ω–æ—Å—Ç–∏? -->\r\n      <div class=\"question-frame\">\r\n        <label>3. –ö–∞–∫ –≤—ã –æ—Ü–µ–Ω–∏—Ç–µ —Å–∫–æ—Ä–æ—Å—Ç—å –æ–±—Å–ª—É–∂–∏–≤–∞–Ω–∏—è? –ë—ã–ª–∏ –ª–∏ —É –≤–∞—Å –∫–∞–∫–∏–µ-–ª–∏–±–æ —Ç—Ä—É–¥–Ω–æ—Å—Ç–∏?</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q3_service_speed\" value=\"–û—á–µ–Ω—å –±—ã—Å—Ç—Ä–æ\" required><div class=\"custom-radio\">1</div>–û—á–µ–Ω—å –±—ã—Å—Ç—Ä–æ</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q3_service_speed\" value=\"–ù–æ—Ä–º–∞–ª—å–Ω–æ\"><div class=\"custom-radio\">2</div>–ù–æ—Ä–º–∞–ª—å–Ω–æ</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q3_service_speed\" value=\"–ú–µ–¥–ª–µ–Ω–Ω–æ\"><div class=\"custom-radio\">3</div>–ú–µ–¥–ª–µ–Ω–Ω–æ</label>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- 4. –ü—Ä–∞–≤–∏–ª—å–Ω–æ –ª–∏ —Å–æ–±—Ä–∞–ª–∏ –≤–∞—à –∑–∞–∫–∞–∑? -->\r\n      <div class=\"question-frame\">\r\n        <label>4. –ü—Ä–∞–≤–∏–ª—å–Ω–æ –ª–∏ —Å–æ–±—Ä–∞–ª–∏ –≤–∞—à –∑–∞–∫–∞–∑?</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q4_order_accuracy\" value=\"–í—Å–µ –≤–µ—Ä–Ω–æ —Å–æ–±—Ä–∞–ª–∏\" required><div class=\"custom-radio\">1</div>–í—Å–µ –≤–µ—Ä–Ω–æ —Å–æ–±—Ä–∞–ª–∏</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q4_order_accuracy\" value=\"–í—Å–µ —Å–æ–±—Ä–∞–ª–∏ –∏ –¥–∞–∂–µ –¥–∞–ª–∏ –¥–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–æ–µ –±–ª—é–¥–æ\"><div class=\"custom-radio\">2</div>–í—Å–µ —Å–æ–±—Ä–∞–ª–∏ –∏ –¥–∞–∂–µ –¥–∞–ª–∏ –¥–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–æ–µ –±–ª—é–¥–æ</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q4_order_accuracy\" value=\"–ù–µ–¥–æ–ª–æ–∂–∏–ª–∏ (–ø—Ä–æ–¥—É–∫—Ç, –∫–æ–Ω–¥–∏–º–µ–Ω—Ç—ã –∏–ª–∏ —Å–∞–ª—Ñ–µ—Ç–∫–∏)\"><div class=\"custom-radio\">3</div>–ù–µ–¥–æ–ª–æ–∂–∏–ª–∏ (–ø—Ä–æ–¥—É–∫—Ç, –∫–æ–Ω–¥–∏–º–µ–Ω—Ç—ã –∏–ª–∏ —Å–∞–ª—Ñ–µ—Ç–∫–∏)</label>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- 5. –ü–æ–Ω—Ä–∞–≤–∏–ª–∏—Å—å –ª–∏ –≤–∞–º –±–ª—é–¥–∞, –∫–æ—Ç–æ—Ä—ã–µ –≤—ã —É –Ω–∞—Å –∑–∞–∫–∞–∑–∞–ª–∏? -->\r\n      <div class=\"question-frame\">\r\n        <label>5. –ü–æ–Ω—Ä–∞–≤–∏–ª–∏—Å—å –ª–∏ –≤–∞–º –±–ª—é–¥–∞, –∫–æ—Ç–æ—Ä—ã–µ –≤—ã —É –Ω–∞—Å –∑–∞–∫–∞–∑–∞–ª–∏?</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q5_dish_satisfaction\" value=\"–î–∞, –≤—Å–µ –≤–∫—É—Å–Ω–æ\" required><div class=\"custom-radio\">1</div>–î–∞, –≤—Å–µ –≤–∫—É—Å–Ω–æ</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q5_dish_satisfaction\" value=\"–ù–∏—á–µ–≥–æ –æ—Å–æ–±–µ–Ω–Ω–æ–≥–æ\"><div class=\"custom-radio\">2</div>–ù–∏—á–µ–≥–æ –æ—Å–æ–±–µ–Ω–Ω–æ–≥–æ</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q5_dish_satisfaction\" value=\"–ù–µ—Ç! –ù–µ –ø–æ–Ω—Ä–∞–≤–∏–ª–æ—Å—å\"><div class=\"custom-radio\">3</div>–ù–µ—Ç! –ù–µ –ø–æ–Ω—Ä–∞–≤–∏–ª–æ—Å—å</label>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- –ü–æ–ª–µ –¥–ª—è –∫–æ–º–º–µ–Ω—Ç–∞—Ä–∏—è -->\r\n      <div class=\"question-frame\">\r\n        <label for=\"comment\">–ö–æ–º–º–µ–Ω—Ç–∞—Ä–∏–π:</label>\r\n        <textarea id=\"comment\" name=\"comment\" rows=\"3\" placeholder=\"–í–∞—à –∫–æ–º–º–µ–Ω—Ç–∞—Ä–∏–π‚Ä¶\"></textarea>\r\n      </div>\r\n      \r\n      <button type=\"submit\" class=\"submit-btn\">–û—Ç–ø—Ä–∞–≤–∏—Ç—å –æ—Ç–∑—ã–≤</button>\r\n    </form>\r\n  </div>\r\n\r\n  <script>\r\n    document.getElementById('feedbackForm').addEventListener('submit', async function(event) {\r\n      event.preventDefault(); // –ü—Ä–µ–¥–æ—Ç–≤—Ä–∞—â–∞–µ–º —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—É—é –æ—Ç–ø—Ä–∞–≤–∫—É —Ñ–æ—Ä–º—ã\r\n\r\n      const btn = document.querySelector(\".submit-btn\");\r\n      if (btn.disabled) return; \r\n\r\n      btn.disabled = true;\r\n      btn.textContent = \"–û—Ç–ø—Ä–∞–≤–∫–∞ –¥–∞–Ω–Ω—ã—Ö...\";\r\n\r\n      try {\r\n        const form = event.target;\r\n        const surveyId = form.surveyId.value; \r\n        const tvHost = form.tvHost.value.trim();\r\n        const comment = form.comment.value.trim();\r\n        \r\n        // --- –°–æ–±–∏—Ä–∞–µ–º –¥–∞–Ω–Ω—ã–µ –¥–ª—è –æ—Ç–ø—Ä–∞–≤–∫–∏ ---\r\n        const formDataToSend = new FormData();\r\n        formDataToSend.append(\"surveyId\", surveyId); \r\n        formDataToSend.append(\"Table visit –ø—Ä–æ–≤–µ–ª\", tvHost);\r\n        formDataToSend.append(\"–ö–æ–º–º–µ–Ω—Ç–∞—Ä–∏–π\", comment); \r\n        formDataToSend.append(\"–í—Ä–µ–º—è –æ—Ç–ø—Ä–∞–≤–∫–∏\", new Date().toLocaleString('ru-RU')); \r\n\r\n        // –°–æ–±–∏—Ä–∞–µ–º –æ—Ç–≤–µ—Ç—ã –Ω–∞ –≤–æ–ø—Ä–æ—Å—ã\r\n        const questionNames = [\r\n          \"q1_first_impression\", \"q2_service_staff\", \"q3_service_speed\", \r\n          \"q4_order_accuracy\", \"q5_dish_satisfaction\"\r\n        ];\r\n\r\n        questionNames.forEach(qName => {\r\n            const selectedValue = document.querySelector(`input[name=\"${qName}\"]:checked`)?.value || \"\";\r\n            formDataToSend.append(qName, selectedValue);\r\n        });\r\n        \r\n        // --- URL –≤–∞—à–µ–≥–æ Apps Script –¥–ª—è –æ–ø—Ä–æ—Å–∞ \"–û–±—Ä–∞—Ç–Ω–∞—è —Å–≤—è–∑—å\" ---\r\n        // !!! –û–ë–Ø–ó–ê–¢–ï–õ–¨–ù–û –ó–ê–ú–ï–ù–ò–¢–ï –≠–¢–û–¢ URL –ù–ê URL –í–ê–®–ï–ì–û –†–ê–ó–í–ï–†–ù–£–¢–û–ì–û APPS SCRIPT –î–õ–Ø –û–ü–†–û–°–ê \"–û–ë–†–ê–¢–ù–ê–Ø –°–í–Ø–ó–¨\" !!!\r\n        const appsScriptUrl = 'https://script.google.com/macros/s/AKfycbw20o-j0l41wOIR4_D_IqPtA5QTweafRDpOjdlTneiDDXyldFObwBRU7F4fkbaVaHj2QA/exec'; \r\n\r\n        // --- –û—Ç–ø—Ä–∞–≤–∫–∞ –¥–∞–Ω–Ω—ã—Ö ---\r\n        const response = await fetch(appsScriptUrl, {\r\n          method: \"POST\",\r\n          body: formDataToSend \r\n        });\r\n\r\n        const result = await response.json(); \r\n\r\n        if (response.ok && result.status === 'success') {\r\n          Swal.fire({\r\n            title: \"–°–ø–∞—Å–∏–±–æ –∑–∞ –æ—Ç–∑—ã–≤!\", \r\n            html: `<b>Table visit –ø—Ä–æ–≤–µ–ª:</b> ${tvHost}<br><b>–ö–æ–º–º–µ–Ω—Ç–∞—Ä–∏–π:</b><br>${comment || \"‚Äî\"}`,\r\n            icon: 'success',\r\n            confirmButtonText: 'OK',\r\n            confirmButtonColor: '#d32f2f' \r\n          });\r\n          form.reset(); \r\n        } else {\r\n          console.error(\"Error sending to Apps Script:\", result);\r\n          Swal.fire({\r\n            title: \"–û—à–∏–±–∫–∞\",\r\n            text: `–ù–µ —É–¥–∞–ª–æ—Å—å –æ—Ç–ø—Ä–∞–≤–∏—Ç—å –¥–∞–Ω–Ω—ã–µ: ${result.message || '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–∞—è –æ—à–∏–±–∫–∞.'}`,\r\n            icon: 'error',\r\n            confirmButtonText: 'OK',\r\n            confirmButtonColor: '#d32f2f'\r\n          });\r\n        }\r\n      } catch (error) {\r\n        console.error(\"–û–±—â–∞—è –æ—à–∏–±–∫–∞:\", error);\r\n        Swal.fire({\r\n          title: \"–û—à–∏–±–∫–∞\",\r\n          text: \"–ü—Ä–æ–∏–∑–æ—à–ª–∞ –Ω–µ–ø—Ä–µ–¥–≤–∏–¥–µ–Ω–Ω–∞—è –æ—à–∏–±–∫–∞. –ü—Ä–æ–≤–µ—Ä—å—Ç–µ –∫–æ–Ω—Å–æ–ª—å.\",\r\n          icon: 'error',\r\n          confirmButtonText: 'OK',\r\n          confirmButtonColor: '#d32f2f'\r\n        });\r\n      } finally {\r\n        btn.disabled = false;\r\n        btn.textContent = \"–û—Ç–ø—Ä–∞–≤–∏—Ç—å –æ—Ç–∑—ã–≤\";\r\n      }\r\n    });\r\n  </script>\r\n</body>\r\n</html>\r\n",
