lue=\"B\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n      \r\n      <style>\r\n.custom-file-upload {\r\n  background-color: #ff4d4d;\r\n  color: white;\r\n  padding: 10px 18px;\r\n  border-radius: 10px;\r\n  display: inline-block;\r\n  cursor: pointer;\r\n  font-weight: bold;\r\n  transition: 0.3s;\r\n}\r\n\r\n.custom-file-upload:hover {\r\n  background-color: #e03e3e;\r\n}\r\n\r\n.custom-file-upload input[type=\"file\"] {\r\n  display: none;\r\n}\r\n</style>\r\n<div style=\"margin-bottom: 20px;\">\r\n  <label style=\"font-weight: bold; display:block; margin-bottom: 5px;\"></label>\r\n\r\n<style>\r\n  .photo-upload {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 10px;\r\n    margin-bottom: 6px;\r\n  }\r\n\r\n  .photo-preview {\r\n    width: 80px;\r\n    height: 80px;\r\n    object-fit: cover;\r\n    border-radius: 8px;\r\n    border: 2px solid #ccc;\r\n  }\r\n\r\n  .upload-label {\r\n    display: inline-block;\r\n    padding: 10px 16px;\r\n    background-color: #d81922;\r\n    color: white;\r\n    border-radius: 8px;\r\n    cursor: pointer;\r\n    font-weight: bold;\r\n  }\r\n\r\n  .upload-label:hover {\r\n    background-color: #a50030;\r\n  }\r\n\r\n  .upload-note {\r\n    font-size: 12px;\r\n    color: #999;\r\n    margin-left: 2px;\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 6px;\r\n    height: 16px;\r\n  }\r\n\r\n  .loader {\r\n    width: 12px;\r\n    height: 12px;\r\n    border: 2px solid #ccc;\r\n    border-top: 2px solid #d1003f;\r\n    border-radius: 50%;\r\n    animation: spin 1s linear infinite;\r\n    display: none;\r\n  }\r\n\r\n  @keyframes spin {\r\n    to {\r\n      transform: rotate(360deg);\r\n    }\r\n  }\r\n</style>\r\n\r\n<div class=\"photo-upload\">\r\n  <label for=\"photo\" class=\"upload-label\">üì∑ –°–¥–µ–ª–∞—Ç—å —Ñ–æ—Ç–æ</label>\r\n  <input type=\"file\" id=\"photo\" accept=\"image/*\" capture=\"environment\" style=\"display: none\">\r\n  <img id=\"preview\" class=\"photo-preview\" src=\"\" alt=\"–§–æ—Ç–æ\">\r\n</div>\r\n\r\n<div class=\"upload-note\">\r\n  <div class=\"loader\" id=\"loader\"></div>\r\n  <span id=\"uploadText\">–§–æ—Ç–æ –∑–∞–≥—Ä—É–∂–∞–µ—Ç—Å—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏</span>\r\n</div>\r\n\r\n<input type=\"hidden\" id=\"photoUrl\" name=\"photoUrl\">\r\n\r\n<script>\r\n  const photoInput = document.getElementById('photo');\r\n  const preview = document.getElementById('preview');\r\n  const photoUrlField = document.getElementById('photoUrl');\r\n  const loader = document.getElementById('loader');\r\n  const uploadText = document.getElementById('uploadText');\r\n\r\n  const uploadUrl = 'https://script.google.com/macros/s/AKfycbzjbYAfHSVAVr4Qyjczvakd-rZF48TkhbaffFZfxT7-_rf-J0suzC9YPw9_CPbPBmgGqw/exec'; // –≤—Å—Ç–∞–≤—å —Å—é–¥–∞ —Å–≤–æ–π —Å–∫—Ä–∏–ø—Ç\r\n\r\n  photoInput.addEventListener('change', async function () {\r\n    const file = photoInput.files[0];\r\n    if (!file) return;\r\n\r\n    const reader = new FileReader();\r\n    reader.onload = () => {\r\n      preview.src = reader.result;\r\n    };\r\n    reader.readAsDataURL(file);\r\n\r\n    // –ü–æ–∫–∞–∑–∞—Ç—å –∞–Ω–∏–º–∞—Ü–∏—é –∑–∞–≥—Ä—É–∑–∫–∏\r\n    loader.style.display = 'inline-block';\r\n    uploadText.textContent = '–ó–∞–≥—Ä—É–∑–∫–∞ —Ñ–æ—Ç–æ...';\r\n\r\n    const readerUpload = new FileReader();\r\n    readerUpload.onloadend = async () => {\r\n      const base64Data = readerUpload.result.split(',')[1];\r\n\r\n      const formData = new FormData();\r\n      formData.append(\"photo\", base64Data);\r\n      formData.append(\"filename\", \"photo_\" + Date.now() + \".jpg\");\r\n      formData.append(\"type\", file.type);\r\n\r\n      try {\r\n        const res = await fetch(uploadUrl, {\r\n          method: \"POST\",\r\n          body: formData,\r\n        });\r\n\r\n        const data = await res.json();\r\n        if (data.success) {\r\n          photoUrlField.value = data.url;\r\n        }\r\n      } catch (err) {\r\n        console.error(\"–û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏ —Ñ–æ—Ç–æ:\", err);\r\n      } finally {\r\n        loader.style.display = 'none';\r\n        uploadText.textContent = '';\r\n      }\r\n    };\r\n    readerUpload.readAsDataURL(file);\r\n  });\r\n</script>\r\n      \r\n      <!-- –ü–æ–ª–µ –¥–ª—è –∫–æ–º–º–µ–Ω—Ç–∞—Ä–∏—è -->\r\n      <div class=\"question-frame\">\r\n        <label for=\"comment\">–ö–æ–º–º–µ–Ω—Ç–∞—Ä–∏–π:</label>\r\n        <textarea id=\"comment\" name=\"comment\" rows=\"4\" placeholder=\"–ù–∞–ø–∏—à–∏—Ç–µ —Å–≤–æ–π –∫–æ–º–º–µ–Ω—Ç–∞—Ä–∏–π...\"></textarea>\r\n      </div>\r\n      \r\n      <button type=\"submit\" class=\"submit-btn\">–ó–∞–∫–æ–Ω—á–∏—Ç—å –æ–ø—Ä–æ—Å</button>\r\n    </form>\r\n  </div>\r\n  \r\n  <script>\r\n    document.getElementById(\"customForm\").addEventListener(\"submit\", async function(event) {\r\n      event.preventDefault(); // –ü—Ä–µ–¥–æ—Ç–≤—Ä–∞—â–∞–µ–º —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—É—é –æ—Ç–ø—Ä–∞–≤–∫—É —Ñ–æ—Ä–º—ã\r\n\r\n      const btn = document.querySelector(\".submit-btn\");\r\n      if (btn.disabled) return; \r\n\r\n      btn.disabled = true;\r\n      btn.textContent = \"–û—Ç–ø—Ä–∞–≤–∫–∞ –¥–∞–Ω–Ω—ã—Ö...\";\r\n\r\n      try {\r\n        const form = event.target;\r\n        const surveyId = form.surveyId.value; \r\n        const yourName = form.yourName.value.trim();\r\n        const employeeName = form.employeeName.value.trim();\r\n        const comment = form.comment.value.trim();\r\n        \r\n        // --- –†–∞—Å—á–µ—Ç –ø—Ä–æ—Ü–µ–Ω—Ç–∞ –∑–Ω–∞–Ω–∏–π ---\r\n        const correctAnswersValues = {\r\n          \"q1_uniform_badge\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q2_badge_approved\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q3_champs_badges\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q4_shoes_black\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q5_no_piercing_jewelry\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q6_grooming\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q7_makeup_calm\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q8_kfc_abbreviation\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q9_express_thoughts\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q10_friendly_contact\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q11_taste_guarantee\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q12_hostess_kpis\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q13_guest_questions\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q14_menu_info\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q15_promo_campaigns\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q16_present_info\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q17_cleaning_procedures\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q18_hall_summer_area_org\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q19_cleanliness_control\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q20_mop_room_control\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q21_glass_floor_table_cleanliness\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q22_equipment_control\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q23_music_volume\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q24_temp_light_change\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q25_promo_materials_check\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q26_employment_questions\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q27_kiosk_demo\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q28_kiosk_malfunction\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q29_offer_kiosk_help\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q30_hand_washing_10_critical_moments\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q31_greet_employees\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q32_check_uniform_standard\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q33_greet_visitors\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q34_find_seats_help\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q35_initiate_conversation\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q36_guest_complaints\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q37_lost_found\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q38_brand_history\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q39_sampling_purpose\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q40_restaurant_intro\": \"–ó–Ω–∞–µ—Ç\"\r\n        };\r\n        \r\n        let score = 0;\r\n        let totalQuestions = 0;\r\n        for (const qName in correctAnswersValues) {\r\n            totalQuestions++;\r\n            const selectedValue = document.querySelector(`input[name=\"${qName}\"]:checked`)?.value;\r\n            if (selectedValue === correctAnswersValues[qName]) {\r\n                score++;\r\n            }\r\n        }\r\n        const percent = Math.round((score / totalQuestions) * 100);\r\n\r\n        // --- –ü–æ–¥–≥–æ—Ç–æ–≤–∫–∞ FormData –¥–ª—è –æ—Ç–ø—Ä–∞–≤–∫–∏ —Ç–æ–ª—å–∫–æ —Ç–µ–∫—Å—Ç–æ–≤—ã—Ö –¥–∞–Ω–Ω—ã—Ö ---\r\n        const formDataToSend = new FormData();\r\n        formDataToSend.append(\"surveyId\", surveyId); \r\n        formDataToSend.append(\"yourName\", yourName);\r\n        formDataToSend.append(\"employeeName\", employeeName);\r\n        formDataToSend.append(\"comment\", comment); \r\n        formDataToSend.append(\"–ü—Ä–æ—Ü–µ–Ω—Ç –∑–Ω–∞–Ω–∏–π\", percent + \"%\"); \r\n\r\n        // –î–æ–±–∞–≤–ª—è–µ–º –æ—Ç–≤–µ—Ç—ã –Ω–∞ –≤–æ–ø—Ä–æ—Å—ã\r\n        for (const qName in correctAnswersValues) {\r\n            const selectedValue = document.querySelector(`input[name=\"${qName}\"]:checked`)?.value || \"\";\r\n            formDataToSend.append(qName, selectedValue);\r\n        }\r\n        \r\n        // --- URL –≤–∞—à–µ–≥–æ Apps Script –¥–ª—è –æ–ø—Ä–æ—Å–∞ \"–•–æ—Å—Ç–µ—Å—Å\" ---\r\n        // !!! –û–ë–Ø–ó–ê–¢–ï–õ–¨–ù–û –ó–ê–ú–ï–ù–ò–¢–ï –≠–¢–û–¢ URL –ù–ê URL –í–ê–®–ï–ì–û –†–ê–ó–í–ï–†–ù–£–¢–û–ì–û APPS SCRIPT –î–õ–Ø –û–ü–†–û–°–ê \"–•–û–°–¢–ï–°–°\" !!!\r\n        const appsScriptUrl = 'https://script.google.com/macros/s/AKfycbxfdjYeqUbkp6MDTPA5jKs-ff65P6nLEHZrn25bSh0WBRFXLzKQBvn_Mqb63bxyPGr_/exec'; \r\n\r\n        // --- –û—Ç–ø—Ä–∞–≤–∫–∞ –¥–∞–Ω–Ω—ã—Ö ---\r\n        const response = await fetch(appsScriptUrl, {\r\n          method: \"POST\",\r\n          body: formDataToSend \r\n        });\r\n\r\n        const result = await response.json(); \r\n\r\n        if (response.ok && result.status === 'success') {\r\n          Swal.fire({\r\n            title: \"–û–ø—Ä–æ—Å - –•–æ—Å—Ç–µ—Å—Å\", \r\n            html: `<b>–°–æ—Ç—Ä—É–¥–Ω–∏–∫:</b> ${employeeName}<br><b>–†–µ–∑—É–ª—å—Ç–∞—Ç:</b> ${percent}%<br><b>–ö–æ–º–º–µ–Ω—Ç–∞—Ä–∏–π:</b><br>${comment}`,\r\n            icon: 'success',\r\n            confirmButtonText: 'OK',\r\n            confirmButtonColor: '#d32f2f'\r\n          });\r\n          form.reset(); \r\n        } else {\r\n          console.error(\"Error sending to Apps Script:\", result);\r\n          Swal.fire({\r\n            title: \"–û—à–∏–±–∫–∞\",\r\n            text: `–ù–µ —É–¥–∞–ª–æ—Å—å –æ—Ç–ø—Ä–∞–≤–∏—Ç—å –¥–∞–Ω–Ω—ã–µ: ${result.message || '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–∞—è –æ—à–∏–±–∫–∞.'}`,\r\n            icon: 'error',\r\n            confirmButtonText: 'OK',\r\n            confirmButtonColor: '#d32f2f'\r\n          });\r\n        }\r\n      } catch (error) {\r\n        console.error(\"–û–±—â–∞—è –æ—à–∏–±–∫–∞:\", error);\r\n        Swal.fire({\r\n          title: \"–û—à–∏–±–∫–∞\",\r\n          text: \"–ü—Ä–æ–∏–∑–æ—à–ª–∞ –Ω–µ–ø—Ä–µ–¥–≤–∏–¥–µ–Ω–Ω–∞—è –æ—à–∏–±–∫–∞. –ü—Ä–æ–≤–µ—Ä—å—Ç–µ –∫–æ–Ω—Å–æ–ª—å.\",\r\n          icon: 'error',\r\n          confirmButtonText: 'OK',\r\n          confirmButtonColor: '#d32f2f'\r\n        });\r\n      } finally {\r\n        btn.disabled = false;\r\n        btn.textContent = \"–ó–∞–∫–æ–Ω—á–∏—Ç—å –æ–ø—Ä–æ—Å\";\r\n      }\r\n    });\r\n  </script>\r\n</body>\r\n</html>",
