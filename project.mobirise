idth: 30px; \r\n            height: 30px;\r\n            font-size: 16px;\r\n        }\r\n        .submit-btn {\r\n            font-size: 16px;\r\n            padding: 12px 20px;\r\n            margin-top: 30px;\r\n        }\r\n    }\r\n\r\n    @media (max-width: 480px) { /* –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –∞–¥–∞–ø—Ç–∞—Ü–∏—è –¥–ª—è –æ—á–µ–Ω—å –º–∞–ª–µ–Ω—å–∫–∏—Ö —ç–∫—Ä–∞–Ω–æ–≤ */\r\n        body { padding: 10px; }\r\n        .form-container { padding: 15px; }\r\n        .form-title { font-size: 22px; margin-bottom: 15px; }\r\n        .avatar-img { width: 120px; height: 120px; }\r\n        .question-frame { margin-bottom: 15px; }\r\n    }\r\n  </style>\r\n  \r\n  <!-- SweetAlert2 –¥–ª—è –∫—Ä–∞—Å–∏–≤—ã—Ö –≤—Å–ø–ª—ã–≤–∞—é—â–∏—Ö –æ–∫–æ–Ω -->\r\n  <script src=\"https://cdn.jsdelivr.net/npm/sweetalert2@11\"></script>\r\n</head>\r\n<body>\r\n  <div class=\"form-container\">\r\n    <img src=\"https://i.imgur.com/SxzkAWe.jpeg\" alt=\"–ê–≤–∞—Ç–∞—Ä\" class=\"avatar-img\">\r\n    <div class=\"form-title\">–û–ø—Ä–æ—Å - –ù–æ–∂–∫–∏ OR</div>\r\n    <form id=\"customForm\">\r\n      <!-- –°–∫—Ä—ã—Ç–æ–µ –ø–æ–ª–µ –¥–ª—è –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ü–∏–∏ –æ–ø—Ä–æ—Å–∞ -->\r\n      <input type=\"hidden\" name=\"surveyId\" value=\"nozhki_or_survey\"> \r\n\r\n      <!-- –ò–º—è HR -->\r\n      <div class=\"question-frame\">\r\n        <label for=\"yourName\">–í–∞—à–µ –∏–º—è:</label>\r\n        <input type=\"text\" id=\"yourName\" name=\"yourName\" required>\r\n      </div>\r\n      \r\n      <!-- –ò–º—è —Å–æ—Ç—Ä—É–¥–Ω–∏–∫–∞ -->\r\n      <div class=\"question-frame\">\r\n        <label for=\"employeeName\">–ò–º—è —Å–æ—Ç—Ä—É–¥–Ω–∏–∫–∞:</label>\r\n        <input type=\"text\" id=\"employeeName\" name=\"employeeName\" required>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 1 -->\r\n      <div class=\"question-frame\">\r\n        <label>1. –û–±—É–≤—å –∑–∞–∫—Ä—ã—Ç–∞—è, –∫–æ–∂–∞–Ω–∞—è, —Ç–µ–º–Ω–∞—è, –Ω–∞ –Ω–µ—Å–∫–æ–ª—å–∑—è—â–µ–π –ø–æ–¥–æ—à–≤–µ. –ù–æ—Å–∫–∏ —Ç–æ–ª—å–∫–æ —á–µ—Ä–Ω–æ–≥–æ —Ü–≤–µ—Ç–∞:</label>\r\n 