ound-color: #fffafa; \r\n      box-shadow: 0 2px 5px rgba(0,0,0,0.05);\r\n    }\r\n    .question-frame label { /* –°—Ç–∏–ª—å –¥–ª—è —Ç–µ–∫—Å—Ç–∞ –≤–æ–ø—Ä–æ—Å–∞/–ø–æ–ª—è –≤–Ω—É—Ç—Ä–∏ —Ä–∞–º–∫–∏ */\r\n        margin-top: 0 !important; \r\n        margin-bottom: 12px !important; \r\n        display: block !important; \r\n        font-weight: bold !important; \r\n        color: #555 !important; \r\n        line-height: 1.4 !important; \r\n        word-wrap: break-word; \r\n        overflow-wrap: break-word; \r\n    }\r\n    label { /* –°–±—Ä–æ—Å –æ–±—â–∏—Ö —Å—Ç–∏–ª–µ–π label, —á—Ç–æ–±—ã –æ–Ω–∏ –Ω–µ –≤–ª–∏—è–ª–∏ –Ω–∞ —ç–ª–µ–º–µ–Ω—Ç—ã –≤–Ω—É—Ç—Ä–∏ —Ä–∞–º–æ–∫ */\r\n        font-weight: normal; \r\n        display: inline; \r\n        margin: 0; \r\n        padding: 0; \r\n        color: inherit;\r\n        line-height: normal;\r\n    }\r\n\r\n    input[type=\"text\"], textarea {\r\n      width: 100%; \r\n      padding: 12px;\r\n      border-radius: 8px;\r\n      border: 1px solid #ccc;\r\n      font-size: 16px;\r\n      margin-bottom: 0; \r\n    }\r\n    textarea {\r\n      resize: vertical; \r\n      min-height: 80px;\r\n    }\r\n    .radio-group {\r\n      display: flex; \r\n      flex-direction: row; /* –í–∞—Ä–∏–∞–Ω—Ç—ã –≤ —Ä—è–¥ */\r\n      gap: 20px; \r\n      margin-bottom: 0; \r\n      flex-wrap: wrap; /* –ü–µ—Ä–µ–Ω–æ—Å –Ω–∞ –Ω–æ–≤—É—é —Å—Ç—Ä–æ–∫—É, –µ—Å–ª–∏ –Ω–µ —Ö–≤–∞—Ç–∞–µ—Ç –º–µ—Å—Ç–∞ */\r\n      justify-content: flex-start; \r\n    }\r\n    .radio-option {\r\n      display: flex;\r\n      align-items: center;\r\n      gap: 12px;\r\n      cursor: pointer;\r\n      padding: 10px;\r\n      border-radius: 8px;\r\n      background-color: transparent; \r\n      transition: background-color 0.2s ease;\r\n      flex-shrink: 0; \r\n    }\r\n    .radio-option:hover {\r\n      background-color: #f0f0f0; \r\n    }\r\n    .radio-option input[type=\"radio\"] {\r\n      display: none; \r\n    }\r\n    .custom-radio {\r\n      flex-shrink: 0; \r\n      width: 35px; \r\n      h