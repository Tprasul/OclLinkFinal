          "_customHTML": "<!DOCTYPE html>\r\n<html lang=\"ru\">\r\n<head>\r\n  <meta charset=\"UTF-8\">\r\n  <title>Калькулятор зарплаты</title>\r\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">\r\n  <style>\r\n    body {\r\n      background-color: #0f0f0f !important;\r\n      margin: 0;\r\n      padding: 20px;\r\n      font-family: 'Arial', sans-serif;\r\n      color: #fff;\r\n    }\r\n\r\n    .salary-form {\r\n      max-width: 500px;\r\n      margin: auto;\r\n      background: #1e1e1e;\r\n      padding: 30px;\r\n      border-radius: 16px;\r\n      color: #fff;\r\n      box-shadow: 0 0 20px rgba(255, 255, 255, 0.05);\r\n    }\r\n\r\n    .salary-form h2 {\r\n      text-align: center;\r\n      margin-bottom: 20px;\r\n      color: #ff9800;\r\n    }\r\n\r\n    .salary-form label {\r\n      display: block;\r\n      margin-top: 15px;\r\n      font-weight: bold;\r\n      color: #ddd;\r\n    }\r\n\r\n    .salary-form select,\r\n    .salary-form input {\r\n      width: 100%;\r\n      padding: 10px;\r\n      margin-top: 5px;\r\n      border-radius: 8px;\r\n      border: 1px solid #444;\r\n      background-color: #2b2b2b;\r\n      color: #fff;\r\n      box-sizing: border-box;\r\n    }\r\n\r\n    .salary-form input::placeholder {\r\n      color: #aaa;\r\n    }\r\n\r\n    .bonus-toggle {\r\n      margin-top: 20px;\r\n    }\r\n\r\n    .bonus-toggle input[type=\"checkbox\"] {\r\n      display: none;\r\n    }\r\n\r\n    .bonus-toggle label {\r\n      display: block;\r\n      width: 100%;\r\n      padding: 10px;\r\n      border-radius: 8px;\r\n      background-color: #2b2b2b;\r\n      text-align: center;\r\n      font-weight: bold;\r\n      cursor: pointer;\r\n      border: 1px solid #555;\r\n      color: #fff;\r\n      transition: 0.3s ease;\r\n    }\r\n\r\n    .bonus-toggle input[type=\"checkbox\"]:checked + label {\r\n      background-color: #ff9800;\r\n      color: #000;\r\n      border-color: #ff9800;\r\n    }\r\n\r\n    .salary-form button {\r\n      margin-top: 20px;\r\n      width: 100%;\r\n      padding: 12px;\r\n      background-color: #ff9800;\r\n      color: black;\r\n      border: none;\r\n      border-radius: 8px;\r\n      font-size: 16px;\r\n      font-weight: bold;\r\n      cursor: pointer;\r\n      transition: 0.2s ease-in-out;\r\n    }\r\n\r\n    .salary-form button:hover {\r\n      background-color: #e68900;\r\n    }\r\n\r\n    .result {\r\n      margin-top: 20px;\r\n      font-size: 16px;\r\n      text-align: left;\r\n      background: #2a2a2a;\r\n      padding: 15px;\r\n      border-radius: 10px;\r\n      border: 1px solid #444;\r\n      color: #fff;\r\n    }\r\n\r\n    .result b {\r\n      color: #ff9800;\r\n    }\r\n  </style>\r\n</head>\r\n<body>\r\n  <div class=\"salary-form\">\r\n    <h2>Калькулятор зарплаты</h2>\r\n\r\n    <label for=\"position\">Выберите должность:</label>\r\n    <select id=\"position\">\r\n      <option value=\"\">-- Выбрать --</option>\r\n      <option value=\"trainer\">Тренер</option>\r\n      <option value=\"trainer2\">Тренер 2</option>\r\n      <option value=\"chk1\">ЧК1</option>\r\n      <option value=\"chk2\">ЧК2</option>\r\n      <option value=\"chk3\">ЧК3</option>\r\n      <option value=\"tehn\">Тех ночь</option>\r\n      <option value=\"tehz\">Тех зал</option>\r\n    </select>\r\n\r\n    <label for=\"dayHours\">Количество дневных часов:</label>\r\n    <input type=\"number\" id=\"dayHours\" placeholder=\"Например: 120\">\r\n\r\n    <label for=\"nightHours\">Количество ночных часов:</label>\r\n    <input type=\"number\" id=\"nightHours\" placeholder=\"Например: 40\">\r\n\r\n    <div class=\"bonus-toggle\">\r\n      <input type=\"checkbox\" id=\"bonus\">\r\n      <label for=\"bonus\">Перфоманс бонус (+3000 сом)</label>\r\n    </div>\r\n\r\n    <button onclick=\"calculate()\">Посчитать зарплату</button>\r\n\r\n    <div class=\"result\" id=\"result\"></div>\r\n  </div>\r\n\r\n  <script>\r\n    const rates = {\r\n      trainer:  { day: 186, night: 246, advance: 8000 },\r\n      trainer2: { day: 191, night: 253, advance: 8000 },\r\n      chk1:     { day: 151, night: 198, advance: 7000 },\r\n      chk2:     { day: 166, night: 218, advance: 7000 },\r\n      chk3:     { day: 171, night: 226, advance: 7000 },\r\n      tehn:     { day: 220, night: 220, advance: 7000 },\r\n      tehz:     { day: 156, night: 203, advance: 7000 }\r\n    };\r\n\r\n    function calculate() {\r\n      const position = document.getElementById('position').value;\r\n      const dayHours = +document.getElementById('dayHours').value || 0;\r\n      const nightHours = +document.getElementById('nightHours').value || 0;\r\n      const hasBonus = document.getElementById('bonus').checked;\r\n\r\n      if (!position || !rates[position]) {\r\n        document.getElementById('result').innerText = 'Пожалуйста, выберите должность.';\r\n        return;\r\n      }\r\n\r\n      const rate = rates[position];\r\n      const dayPay = rate.day * dayHours;\r\n      const nightPay = rate.night * nightHours;\r\n      const bonus = hasBonus ? 3000 : 0;\r\n      const advance = rate.advance || 0;\r\n      const gross = dayPay + nightPay + bonus;\r\n      const net = gross - advance;\r\n\r\n      document.getElementById('result').innerHTML = `\r\n        Дневные часы: ${dayHours} ч × ${rate.day} сом = <b>${dayPay.toLocaleString()} сом</b><br>\r\n        Ночные часы: ${nightHours} ч × ${rate.night} сом = <b>${nightPay.toLocaleString()} сом</b><br>\r\n        Бонус: <b>${bonus.toLocaleString()} сом</b><br>\r\n        Аванс: -<b>${advance.toLocaleString()} сом</b><br><br>\r\n        <span style=\"font-size: 20px;\">💰 <b>Итого к выплате: ${Math.max(0, Math.round(net)).toLocaleString()} сом</b></span>\r\n      `;\r\n    }\r\n  </script>\r\n</body>\r\n</html>\r\n",
