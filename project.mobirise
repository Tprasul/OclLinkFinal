          "_name": "custom-html",
          "_sourceTheme": "startm5",
          "_customHTML": "<section class=\"section-dark cid-uNrc7urH9B\" style=\"padding: 60px 20px;\" id=\"custom-html-4y\">\r\n  <h2 style=\"text-align: center; font-size: 32px; font-weight: bold; color: white; margin-bottom: 40px;\">\r\n    üì∏ –ì–∞–ª–µ—Ä–µ—è —Å–æ—Ç—Ä—É–¥–Ω–∏–∫–æ–≤\r\n  </h2>\r\n\r\n  <div class=\"row-slider-wrapper\" id=\"gallery-rows\">\r\n    <div class=\"row-slider\" data-direction=\"left\"></div>\r\n    <div class=\"row-slider\" data-direction=\"right\"></div>\r\n    <div class=\"row-slider\" data-direction=\"left\"></div>\r\n  </div>\r\n</section>\r\n\r\n<style>\r\n  .section-dark {\r\n    background: linear-gradient(to right, #0f2027, #203a43, #2c5364);\r\n  }\r\n\r\n  .row-slider-wrapper {\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 10px;\r\n  }\r\n\r\n  .row-slider {\r\n    display: flex;\r\n    gap: 6px; /* –º–∏–Ω–∏–º–∞–ª—å–Ω—ã–π –æ—Ç—Å—Ç—É–ø –º–µ–∂–¥—É —Ñ–æ—Ç–æ */\r\n    overflow: hidden;\r\n    position: relative;\r\n    height: 240px;\r\n    scrollbar-width: none; /* Firefox */\r\n  }\r\n\r\n  .row-slider::-webkit-scrollbar {\r\n    display: none; /* Chrome/Safari */\r\n  }\r\n\r\n  .slider-item {\r\n    flex: none;\r\n    width: 240px;\r\n    height: 100%;\r\n    border-radius: 12px;\r\n    overflow: hidden;\r\n    box-shadow: 0 2px 8px rgba(0,0,0,0.2);\r\n    animation: fadeIn 0.5s ease forwards;\r\n  }\r\n\r\n  .slider-item img {\r\n    width: 100%;\r\n    height: 100%;\r\n    object-fit: cover;\r\n    display: block;\r\n    border-radius: 12px;\r\n  }\r\n\r\n  @keyframes fadeIn {\r\n    from { opacity: 0; transform: translateY(20px); }\r\n    to { opacity: 1; transform: translateY(0); }\r\n  }\r\n</style>\r\n\r\n<script>\r\n  function shuffle(array) {\r\n    for (let i = array.length - 1; i > 0; i--) {\r\n      const j = Math.floor(Math.random() * (i + 1));\r\n      [array[i], array[j]] = [array[j], array[i]];\r\n    }\r\n  }\r\n\r\n  function duplicateImages(images, times = 2) {\r\n    const result = [];\r\n    for (let i = 0; i < times; i++) {\r\n      result.push(...images.map(img => img.cloneNode(true)));\r\n    }\r\n    return result;\r\n  }\r\n\r\n  function animateRow(row, direction) {\r\n    const speed = 0.5;\r\n    function step() {\r\n      if (direction === \"left\") {\r\n        row.scrollLeft += speed;\r\n        if (row.scrollLeft >= row.scrollWidth / 2) {\r\n          row.scrollLeft = 0;\r\n        }\r\n      } else {\r\n        row.scrollLeft -= speed;\r\n        if (row.scrollLeft <= 0) {\r\n          row.scrollLeft = row.scrollWidth / 2;\r\n        }\r\n      }\r\n      requestAnimationFrame(step);\r\n    }\r\n    step();\r\n  }\r\n\r\n  fetch(\"https://script.google.com/macros/s/AKfycbyX7rk3Hw1CBA0r93ns_43XEj68pgFTU9IAJaWwvT2nVOpEHDylReRzSrnQLGRTStP_/exec\")\r\n    .then(res => res.json())\r\n    .then(data => {\r\n      const last60 = data.slice(-60); // 20 —Ñ–æ—Ç–æ –Ω–∞ —Ä—è–¥\r\n      shuffle(last60);\r\n\r\n      const rows = document.querySelectorAll(\".row-slider\");\r\n      rows.forEach((row, index) => {\r\n        const direction = row.dataset.direction;\r\n        const slice = last60.slice(index * 20, (index + 1) * 20);\r\n        const items = slice.map(entry => {\r\n          const div = document.createElement(\"div\");\r\n          div.className = \"slider-item\";\r\n          div.innerHTML = `<img src=\"${entry.url}\" alt=\"–§–æ—Ç–æ\" referrerpolicy=\"no-referrer\">`;\r\n          return div;\r\n        });\r\n\r\n        const looped = duplicateImages(items);\r\n        looped.forEach(el => row.appendChild(el));\r\n\r\n        animateRow(row, direction);\r\n      });\r\n    });\r\n</script>\r\n",
