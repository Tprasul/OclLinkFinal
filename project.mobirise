          "_customHTML": "<!DOCTYPE html>\r\n<html lang=\"ru\">\r\n<head>\r\n  <meta charset=\"UTF-8\">\r\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n  <title>–û–ø—Ä–æ—Å - –ù–∞–ø–∏—Ç–æ—á–Ω–∏–∫</title>\r\n  \r\n  <!-- –°—Ç–∏–ª–∏ -->\r\n  <style>\r\n    /* –ì–ª–æ–±–∞–ª—å–Ω—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –¥–ª—è –æ—Ç–∑—ã–≤—á–∏–≤–æ–≥–æ –¥–∏–∑–∞–π–Ω–∞ */\r\n    *, *::before, *::after {\r\n      box-sizing: border-box;\r\n    }\r\n\r\n    body {\r\n      background-color: #f8f8f8;\r\n      display: flex;\r\n      flex-direction: column; /* –≠–ª–µ–º–µ–Ω—Ç—ã –≤ —Å—Ç–æ–ª–±–∏–∫, –¥–ª—è —Ü–µ–Ω—Ç—Ä–∏—Ä–æ–≤–∞–Ω–∏—è */\r\n      justify-content: center;\r\n      align-items: center;\r\n      min-height: 100vh;\r\n      margin: 0;\r\n      padding: 20px; /* –û—Ç—Å—Ç—É–ø –ø–æ –∫—Ä–∞—è–º –¥–ª—è –∞–¥–∞–ø—Ç–∏–≤–Ω–æ—Å—Ç–∏ */\r\n      font-family: sans-serif;\r\n    }\r\n    .form-container {\r\n      max-width: 600px;\r\n      width: 100%; /* –ó–∞–Ω–∏–º–∞–µ—Ç –≤—Å—é –¥–æ—Å—Ç—É–ø–Ω—É—é —à–∏—Ä–∏–Ω—É –¥–æ max-width */\r\n      padding: 30px;\r\n      border: 1px solid #ddd;\r\n      border-radius: 12px;\r\n      background-color: #FFE4E1; /* –°–≤–µ—Ç–ª–æ-—Ä–æ–∑–æ–≤—ã–π */\r\n      box-shadow: 0 8px 20px rgba(0,0,0,0.1);\r\n    }\r\n    .avatar-img {\r\n      width: 150px;\r\n      height: 150px;\r\n      border-radius: 50%;\r\n      object-fit: cover;\r\n      margin: 0 auto 20px auto; /* –¶–µ–Ω—Ç—Ä–∏—Ä–æ–≤–∞–Ω–∏–µ */\r\n      display: block;\r\n      border: 3px solid #d32f2f; /* –¶–≤–µ—Ç –∫–∞–∫ —É –∫–Ω–æ–ø–∫–∏ */\r\n      box-shadow: 0 0 15px rgba(0,0,0,0.15);\r\n    }\r\n    .form-title {\r\n      font-size: 28px;\r\n      font-weight: bold;\r\n      text-align: center;\r\n      margin-bottom: 30px;\r\n      color: #333;\r\n    }\r\n    .question-frame { /* –†–∞–º–∫–∞ –¥–ª—è –≤–æ–ø—Ä–æ—Å–æ–≤ –∏ –ø–æ–ª–µ–π –≤–≤–æ–¥–∞ */\r\n      border: 1px solid #ffcccb; \r\n      border-radius: 8px;\r\n      padding: 15px;\r\n      margin-bottom: 25px; \r\n      background-color: #fffafa; \r\n      box-shadow: 0 2px 5px rgba(0,0,0,0.05);\r\n    }\r\n    .question-frame label { /* –°—Ç–∏–ª—å –¥–ª—è —Ç–µ–∫—Å—Ç–∞ –≤–æ–ø—Ä–æ—Å–∞/–ø–æ–ª—è –≤–Ω—É—Ç—Ä–∏ —Ä–∞–º–∫–∏ */\r\n        margin-top: 0; \r\n        margin-bottom: 12px; \r\n        display: block; \r\n        font-weight: bold; \r\n        color: #555; \r\n        line-height: 1.4; \r\n        word-wrap: break-word; \r\n        overflow-wrap: break-word; \r\n    }\r\n\r\n    input[type=\"text\"], textarea {\r\n      width: 100%; \r\n      padding: 12px;\r\n      border-radius: 8px;\r\n      border: 1px solid #ccc;\r\n      font-size: 16px;\r\n      margin-bottom: 0; \r\n    }\r\n    textarea {\r\n      resize: vertical; \r\n      min-height: 80px;\r\n    }\r\n    .radio-group {\r\n      display: flex; \r\n      flex-direction: row; /* –í–∞—Ä–∏–∞–Ω—Ç—ã –≤ —Ä—è–¥ */\r\n      gap: 20px; \r\n      margin-bottom: 0; \r\n      flex-wrap: wrap; /* –ü–µ—Ä–µ–Ω–æ—Å –Ω–∞ –Ω–æ–≤—É—é —Å—Ç—Ä–æ–∫—É, –µ—Å–ª–∏ –Ω–µ —Ö–≤–∞—Ç–∞–µ—Ç –º–µ—Å—Ç–∞ */\r\n      justify-content: flex-start; /* –í—ã—Ä–∞–≤–Ω–∏–≤–∞–Ω–∏–µ –ø–æ –ª–µ–≤–æ–º—É –∫—Ä–∞—é */\r\n    }\r\n    .radio-option {\r\n      display: flex;\r\n      align-items: center;\r\n      gap: 12px;\r\n      cursor: pointer;\r\n      padding: 10px;\r\n      border-radius: 8px;\r\n      background-color: transparent; /* –ü—Ä–æ–∑—Ä–∞—á–Ω—ã–π —Ñ–æ–Ω */\r\n      transition: background-color 0.2s ease;\r\n      flex-shrink: 0; /* –ù–µ —Å–∂–∏–º–∞—Ç—å */\r\n    }\r\n    .radio-option:hover {\r\n      background-color: #f0f0f0; /* –õ–µ–≥–∫–∏–π —Ñ–æ–Ω –ø—Ä–∏ –Ω–∞–≤–µ–¥–µ–Ω–∏–∏ */\r\n    }\r\n    .radio-option input[type=\"radio\"] {\r\n      display: none; /* –°–∫—Ä—ã–≤–∞–µ–º —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–µ —Ä–∞–¥–∏–æ-–∫–Ω–æ–ø–∫–∏ */\r\n    }\r\n    .custom-radio {\r\n      flex-shrink: 0; \r\n      width: 35px; \r\n      height: 35px;\r\n      background-color: #eee;\r\n      border-radius: 50%;\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: center;\r\n      font-weight: bold;\r\n      color: #333;\r\n      border: 2px solid transparent;\r\n      transition: 0.2s;\r\n      font-size: 18px;\r\n    }\r\n    .radio-option input[type=\"radio\"]:checked + .custom-radio {\r\n      border-color: #d32f2f;\r\n      background-color: #d32f2f;\r\n      color: white;\r\n      box-shadow: 0 0 10px rgba(211, 47, 47, 0.5);\r\n    }\r\n    .submit-btn {\r\n      margin-top: 40px;\r\n      padding: 15px 25px;\r\n      background-color: #d32f2f;\r\n      color: white;\r\n      font-size: 18px;\r\n      font-weight: bold;\r\n      border: none;\r\n      border-radius: 10px;\r\n      cursor: pointer;\r\n      width: 100%;\r\n      transition: background-color 0.3s ease, transform 0.2s ease;\r\n      box-shadow: 0 4px 10px rgba(0,0,0,0.3);\r\n    }\r\n    .submit-btn:hover:not(:disabled) {\r\n      background-color: #b71c1c;\r\n      transform: translateY(-2px);\r\n    }\r\n    .submit-btn:disabled {\r\n      background-color: #ff7f7f; \r\n      cursor: not-allowed;\r\n      box-shadow: none;\r\n    }\r\n    /* #cameraInput style is no longer needed as the input field is removed */\r\n  </style>\r\n  \r\n  <!-- SweetAlert2 –¥–ª—è –∫—Ä–∞—Å–∏–≤—ã—Ö –≤—Å–ø–ª—ã–≤–∞—é—â–∏—Ö –æ–∫–æ–Ω -->\r\n  <script src=\"https://cdn.jsdelivr.net/npm/sweetalert2@11\"></script>\r\n</head>\r\n<body>\r\n  \r\n  <div class=\"form-container\">\r\n    <img src=\"https://i.imgur.com/SxzkAWe.jpeg\" alt=\"–ê–≤–∞—Ç–∞—Ä\" class=\"avatar-img\">\r\n    \r\n    <div class=\"form-title\">–û–ø—Ä–æ—Å - –ù–∞–ø–∏—Ç–æ—á–Ω–∏–∫</div>\r\n\r\n    <form id=\"customForm\">\r\n      <!-- –°–∫—Ä—ã—Ç–æ–µ –ø–æ–ª–µ –¥–ª—è –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ü–∏–∏ –æ–ø—Ä–æ—Å–∞ -->\r\n      <input type=\"hidden\" name=\"surveyId\" value=\"napitochnik_survey\"> \r\n\r\n      <!-- –ò–º—è HR -->\r\n      <div class=\"question-frame\">\r\n        <label for=\"yourName\">–í–∞—à–µ –∏–º—è:</label>\r\n        <input type=\"text\" id=\"yourName\" name=\"yourName\" required>\r\n      </div>\r\n      \r\n      <!-- –ò–º—è —Å–æ—Ç—Ä—É–¥–Ω–∏–∫–∞ -->\r\n      <div class=\"question-frame\">\r\n        <label for=\"employeeName\">–ò–º—è —Å–æ—Ç—Ä—É–¥–Ω–∏–∫–∞:</label>\r\n        <input type=\"text\" id=\"employeeName\" name=\"employeeName\" required>\r\n      </div>\r\n      \r\n      <!-- –í–æ–ø—Ä–æ—Å 1 -->\r\n      <div class=\"question-frame\">\r\n        <label>1. –ù–µ—Ç –≤–∏–¥–∏–º—ã—Ö –ø—Ä–∏–∑–Ω–∞–∫–æ–≤ –∑–∞–±–æ–ª–µ–≤–∞–Ω–∏—è, –∫–æ—Ç–æ—Ä—ã–µ –º–æ–≥—É—Ç –ø–æ—Å–ª—É–∂–∏—Ç—å –ø—Ä–∏—á–∏–Ω–æ–π –¥–ª—è –æ—Ç—Å—Ç—Ä–∞–Ω–µ–Ω–∏—è –æ—Ç —Ä–∞–±–æ—Ç—ã:</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q1_health_signs\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q1_health_signs\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 2 -->\r\n      <div class=\"question-frame\">\r\n        <label>2. –ú–æ–µ—Ç —Ä—É–∫–∏ —Å–æ–≥–ª–∞—Å–Ω–æ –ø—Ä–∞–≤–∏–ª–∞–º –º—ã—Ç—å—è —Ä—É–∫, –ø—Ä–∏ –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ—Å—Ç–∏ –∏—Å–ø–æ–ª—å–∑—É–µ—Ç –ø–µ—Ä—á–∞—Ç–∫–∏ –ø—Ä–∏ —Ä–∞–±–æ—Ç–µ —Å –ø—Ä–æ–¥—É–∫—Ç–æ–º:</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q2_hand_washing\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q2_hand_washing\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 3 -->\r\n      <div class=\"question-frame\">\r\n        <label>3. –û–±–µ—Å–ø–µ—á–∏–≤–∞–µ—Ç –æ—Ç—Å—É—Ç—Å—Ç–≤–∏–µ –ø–µ—Ä–µ–∫—Ä–µ—Å—Ç–Ω–æ–≥–æ –∑–∞–≥—Ä—è–∑–Ω–µ–Ω–∏—è:</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q3_cross_contamination\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q3_cross_contamination\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 4 -->\r\n      <div class=\"question-frame\">\r\n        <label>4. –°–æ–±–ª—é–¥–∞–µ—Ç –ø—Ä–æ—á–∏–µ —Å—Ç–∞–Ω–¥–∞—Ä—Ç—ã –ø–∏—â–µ–≤–æ–π –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç–∏:</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q4_food_safety_standards\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q4_food_safety_standards\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 5 -->\r\n      <div class=\"question-frame\">\r\n        <label>5. –ü—Ä–æ–≤–µ—Ä—è–µ—Ç —Å—Ä–æ–∫–∏ –≥–æ–¥–Ω–æ—Å—Ç–∏, –ø—Ä–æ—Å—Ä–æ—á–µ–Ω–Ω—ã–π –ø—Ä–æ–¥—É–∫—Ç —Å–ø–∏—Å—ã–≤–∞–µ—Ç. –ù–µ–¥–æ—Å—Ç–∞—é—â–∏–µ –ø—Ä–æ–¥—É–∫—Ç—ã –ø–æ–ø–æ–ª–Ω—è–µ—Ç, –∏—Å–ø–æ–ª—å–∑—É—è –ø—Ä–∞–≤–∏–ª–æ —Ä–æ—Ç–∞—Ü–∏–∏ –ø—Ä–æ–¥—É–∫—Ç–æ–≤:</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q5_shelf_life_rotation\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q5_shelf_life_rotation\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 6 -->\r\n      <div class=\"question-frame\">\r\n        <label>6. –°–æ–±–ª—é–¥–∞–µ—Ç —Ç–µ—Ö–Ω–∏–∫—É –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç–∏ –ø—Ä–∏ —Ä–∞–±–æ—Ç–µ —Å –æ–±–æ—Ä—É–¥–æ–≤–∞–Ω–∏–µ–º:</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q6_equipment_safety\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q6_equipment_safety\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 7 -->\r\n      <div class=\"question-frame\">\r\n        <label>7. –°–æ–æ–±—â–∞–µ—Ç –º–µ–Ω–µ–¥–∂–µ—Ä—É –æ –ª—é–±—ã—Ö –Ω–µ–∏—Å–ø—Ä–∞–≤–Ω–æ—Å—Ç—è—Ö –æ–±–æ—Ä—É–¥–æ–≤–∞–Ω–∏—è:</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q7_report_malfunctions\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q7_report_malfunctions\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 8 -->\r\n      <div class=\"question-frame\">\r\n        <label>8. –ó–Ω–∞–µ—Ç —Å–≤–æ–∏ —Ü–µ–ª–∏ –∏ –∑–∞–¥–∞—á–∏ –Ω–∞ —Å–º–µ–Ω—É:</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q8_shift_goals\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q8_shift_goals\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 9 -->\r\n      <div class=\"question-frame\">\r\n        <label>9. –ü—Ä–æ—Ü–µ–¥—É—Ä—ã –ø—Ä–∏–≥–æ—Ç–æ–≤–ª–µ–Ω–∏—è –Ω–∞–ø–∏—Ç–∫–æ–≤ / –º–æ—Ä–æ–∂–µ–Ω–æ–≥–æ / —Ä–∞–∑–æ–≥—Ä–µ–≤–∞ –¥–µ—Å–µ—Ä—Ç–æ–≤ —Å–æ–±–ª—é–¥–∞—é—Ç—Å—è:</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q9_beverage_prep_proc\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q9_beverage_prep_proc\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 10 -->\r\n      <div class=\"question-frame\">\r\n        <label>10. –ö–æ–Ω—Ç—Ä–æ–ª–∏—Ä—É–µ—Ç –∫–∞—á–µ—Å—Ç–≤–æ –ø—Ä–æ–¥—É–∫—Ç–∞:</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q10_product_quality_control\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q10_product_quality_control\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 11 -->\r\n      <div class=\"question-frame\">\r\n        <label>11. –ü—Ä–æ—Ü–µ–¥—É—Ä—ã —É–ø–∞–∫–æ–≤–∫–∏ –¥–µ—Å–µ—Ä—Ç–æ–≤ / —Å–µ—Ä–≤–∏—Ä–æ–≤–∫–∏ –Ω–∞–ø–∏—Ç–∫–æ–≤ —Å–æ–±–ª—é–¥–∞—é—Ç—Å—è:</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q11_packaging_serving_proc\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q11_packaging_serving_proc\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 12 -->\r\n      <div class=\"question-frame\">\r\n        <label>12. –ü–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ—Ç —Ä–∞–±–æ—á—É—é —Å—Ç–∞–Ω—Ü–∏—é –≤ —á–∏—Å—Ç–æ—Ç–µ –≤ —Ç–µ—á–µ–Ω–∏–µ –≤—Å–µ–π —Å–º–µ–Ω—ã:</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q12_station_cleanliness\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q12_station_cleanliness\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 13 -->\r\n      <div class=\"question-frame\">\r\n        <label>13. –§–æ–∫—É—Å –Ω–∞ —Å–∫–æ—Ä–æ—Å—Ç—å –æ–±—Å–ª—É–∂–∏–≤–∞–Ω–∏—è:</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q13_service_speed_focus\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q13_service_speed_focus\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 14 -->\r\n      <div class=\"question-frame\">\r\n        <label>14. –≠—Ñ—Ñ–µ–∫—Ç–∏–≤–Ω–∞—è –∫–æ–º–º—É–Ω–∏–∫–∞—Ü–∏—è —Å–æ –°–∫–∞—É—Ç–æ–º –∏ –ú–µ–Ω–µ–¥–∂–µ—Ä–æ–º:</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q14_effective_communication\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q14_effective_communication\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n      \r\n      <style>\r\n.custom-file-upload {\r\n  background-color: #ff4d4d;\r\n  color: white;\r\n  padding: 10px 18px;\r\n  border-radius: 10px;\r\n  display: inline-block;\r\n  cursor: pointer;\r\n  font-weight: bold;\r\n  transition: 0.3s;\r\n}\r\n\r\n.custom-file-upload:hover {\r\n  background-color: #e03e3e;\r\n}\r\n\r\n.custom-file-upload input[type=\"file\"] {\r\n  display: none;\r\n}\r\n</style>\r\n<div style=\"margin-bottom: 20px;\">\r\n  <label style=\"font-weight: bold; display:block; margin-bottom: 5px;\"></label>\r\n\r\n<style>\r\n  .photo-upload {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 10px;\r\n    margin-bottom: 6px;\r\n  }\r\n\r\n  .photo-preview {\r\n    width: 80px;\r\n    height: 80px;\r\n    object-fit: cover;\r\n    border-radius: 8px;\r\n    border: 2px solid #ccc;\r\n  }\r\n\r\n  .upload-label {\r\n    display: inline-block;\r\n    padding: 10px 16px;\r\n    background-color: #d81922;\r\n    color: white;\r\n    border-radius: 8px;\r\n    cursor: pointer;\r\n    font-weight: bold;\r\n  }\r\n\r\n  .upload-label:hover {\r\n    background-color: #a50030;\r\n  }\r\n\r\n  .upload-note {\r\n    font-size: 12px;\r\n    color: #999;\r\n    margin-left: 2px;\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 6px;\r\n    height: 16px;\r\n  }\r\n\r\n  .loader {\r\n    width: 12px;\r\n    height: 12px;\r\n    border: 2px solid #ccc;\r\n    border-top: 2px solid #d1003f;\r\n    border-radius: 50%;\r\n    animation: spin 1s linear infinite;\r\n    display: none;\r\n  }\r\n\r\n  @keyframes spin {\r\n    to {\r\n      transform: rotate(360deg);\r\n    }\r\n  }\r\n</style>\r\n\r\n<div class=\"photo-upload\">\r\n  <label for=\"photo\" class=\"upload-label\">üì∑ –°–¥–µ–ª–∞—Ç—å —Ñ–æ—Ç–æ</label>\r\n  <input type=\"file\" id=\"photo\" accept=\"image/*\" capture=\"environment\" style=\"display: none\">\r\n  <img id=\"preview\" class=\"photo-preview\" src=\"\" alt=\"–§–æ—Ç–æ\">\r\n</div>\r\n\r\n<div class=\"upload-note\">\r\n  <div class=\"loader\" id=\"loader\"></div>\r\n  <span id=\"uploadText\">–§–æ—Ç–æ –∑–∞–≥—Ä—É–∂–∞–µ—Ç—Å—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏</span>\r\n</div>\r\n\r\n<input type=\"hidden\" id=\"photoUrl\" name=\"photoUrl\">\r\n\r\n<script>\r\n  const photoInput = document.getElementById('photo');\r\n  const preview = document.getElementById('preview');\r\n  const photoUrlField = document.getElementById('photoUrl');\r\n  const loader = document.getElementById('loader');\r\n  const uploadText = document.getElementById('uploadText');\r\n\r\n  const uploadUrl = 'https://script.google.com/macros/s/AKfycbzjbYAfHSVAVr4Qyjczvakd-rZF48TkhbaffFZfxT7-_rf-J0suzC9YPw9_CPbPBmgGqw/exec'; // –≤—Å—Ç–∞–≤—å —Å—é–¥–∞ —Å–≤–æ–π —Å–∫—Ä–∏–ø—Ç\r\n\r\n  photoInput.addEventListener('change', async function () {\r\n    const file = photoInput.files[0];\r\n    if (!file) return;\r\n\r\n    const reader = new FileReader();\r\n    reader.onload = () => {\r\n      preview.src = reader.result;\r\n    };\r\n    reader.readAsDataURL(file);\r\n\r\n    // –ü–æ–∫–∞–∑–∞—Ç—å –∞–Ω–∏–º–∞—Ü–∏—é –∑–∞–≥—Ä—É–∑–∫–∏\r\n    loader.style.display = 'inline-block';\r\n    uploadText.textContent = '–ó–∞–≥—Ä—É–∑–∫–∞ —Ñ–æ—Ç–æ...';\r\n\r\n    const readerUpload = new FileReader();\r\n    readerUpload.onloadend = async () => {\r\n      const base64Data = readerUpload.result.split(',')[1];\r\n\r\n      const formData = new FormData();\r\n      formData.append(\"photo\", base64Data);\r\n      formData.append(\"filename\", \"photo_\" + Date.now() + \".jpg\");\r\n      formData.append(\"type\", file.type);\r\n\r\n      try {\r\n        const res = await fetch(uploadUrl, {\r\n          method: \"POST\",\r\n          body: formData,\r\n        });\r\n\r\n        const data = await res.json();\r\n        if (data.success) {\r\n          photoUrlField.value = data.url;\r\n        }\r\n      } catch (err) {\r\n        console.error(\"–û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏ —Ñ–æ—Ç–æ:\", err);\r\n      } finally {\r\n        loader.style.display = 'none';\r\n        uploadText.textContent = '';\r\n      }\r\n    };\r\n    readerUpload.readAsDataURL(file);\r\n  });\r\n</script>\r\n      \r\n      <!-- –ü–æ–ª–µ –¥–ª—è –∫–æ–º–º–µ–Ω—Ç–∞—Ä–∏—è -->\r\n      <div class=\"question-frame\">\r\n        <label for=\"comment\">–ö–æ–º–º–µ–Ω—Ç–∞—Ä–∏–π:</label>\r\n        <textarea id=\"comment\" name=\"comment\" rows=\"4\" placeholder=\"–ù–∞–ø–∏—à–∏—Ç–µ —Å–≤–æ–π –∫–æ–º–º–µ–Ω—Ç–∞—Ä–∏–π...\"></textarea>\r\n      </div>\r\n      \r\n      <button type=\"submit\" class=\"submit-btn\">–ó–∞–∫–æ–Ω—á–∏—Ç—å –æ–ø—Ä–æ—Å</button>\r\n    </form>\r\n  </div>\r\n  \r\n  <script>\r\n    document.getElementById(\"customForm\").addEventListener(\"submit\", async function(event) {\r\n      event.preventDefault(); // –ü—Ä–µ–¥–æ—Ç–≤—Ä–∞—â–∞–µ–º —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—É—é –æ—Ç–ø—Ä–∞–≤–∫—É —Ñ–æ—Ä–º—ã\r\n\r\n      const btn = document.querySelector(\".submit-btn\");\r\n      if (btn.disabled) return; \r\n\r\n      btn.disabled = true;\r\n      btn.textContent = \"–û—Ç–ø—Ä–∞–≤–∫–∞ –¥–∞–Ω–Ω—ã—Ö...\";\r\n\r\n      try {\r\n        const form = event.target;\r\n        const surveyId = form.surveyId.value; // –ü–æ–ª—É—á–∞–µ–º ID –æ–ø—Ä–æ—Å–∞\r\n        const yourName = form.yourName.value.trim();\r\n        const employeeName = form.employeeName.value.trim();\r\n        const comment = form.comment.value.trim();\r\n        \r\n        // --- –†–∞—Å—á–µ—Ç –ø—Ä–æ—Ü–µ–Ω—Ç–∞ –∑–Ω–∞–Ω–∏–π ---\r\n        const correctAnswersValues = {\r\n          \"q1_health_signs\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q2_hand_washing\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q3_cross_contamination\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q4_food_safety_standards\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q5_shelf_life_rotation\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q6_equipment_safety\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q7_report_malfunctions\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q8_shift_goals\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q9_beverage_prep_proc\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q10_product_quality_control\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q11_packaging_serving_proc\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q12_station_cleanliness\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q13_service_speed_focus\": \"–ó–Ω–∞–µ—Ç\", \r\n          \"q14_effective_communication\": \"–ó–Ω–∞–µ—Ç\" \r\n        };\r\n        \r\n        let score = 0;\r\n        let totalQuestions = 0;\r\n        for (const qName in correctAnswersValues) {\r\n            totalQuestions++;\r\n            const selectedValue = document.querySelector(`input[name=\"${qName}\"]:checked`)?.value;\r\n            if (selectedValue === correctAnswersValues[qName]) {\r\n                score++;\r\n            }\r\n        }\r\n        const percent = Math.round((score / totalQuestions) * 100);\r\n\r\n        // --- –ü–æ–¥–≥–æ—Ç–æ–≤–∫–∞ FormData –¥–ª—è –æ—Ç–ø—Ä–∞–≤–∫–∏ —Ç–æ–ª—å–∫–æ —Ç–µ–∫—Å—Ç–æ–≤—ã—Ö –¥–∞–Ω–Ω—ã—Ö ---\r\n        const formDataToSend = new FormData();\r\n        formDataToSend.append(\"surveyId\", surveyId); \r\n        formDataToSend.append(\"yourName\", yourName);\r\n        formDataToSend.append(\"employeeName\", employeeName);\r\n        formDataToSend.append(\"comment\", comment); \r\n        formDataToSend.append(\"–ü—Ä–æ—Ü–µ–Ω—Ç –∑–Ω–∞–Ω–∏–π\", percent + \"%\"); \r\n\r\n        // –î–æ–±–∞–≤–ª—è–µ–º –æ—Ç–≤–µ—Ç—ã –Ω–∞ –≤–æ–ø—Ä–æ—Å—ã\r\n        for (const qName in correctAnswersValues) {\r\n            const selectedValue = document.querySelector(`input[name=\"${qName}\"]:checked`)?.value || \"\";\r\n            formDataToSend.append(qName, selectedValue);\r\n        }\r\n        \r\n        // --- URL –≤–∞—à–µ–≥–æ Apps Script –¥–ª—è –æ–ø—Ä–æ—Å–∞ \"–ù–∞–ø–∏—Ç–æ—á–Ω–∏–∫\" (—Ç–æ–ª—å–∫–æ —Ç–µ–∫—Å—Ç) ---\r\n        // !!! –û–ë–Ø–ó–ê–¢–ï–õ–¨–ù–û –ó–ê–ú–ï–ù–ò–¢–ï –≠–¢–û–¢ URL –ù–ê URL –í–ê–®–ï–ì–û –†–ê–ó–í–ï–†–ù–£–¢–û–ì–û APPS SCRIPT –î–õ–Ø –û–ü–†–û–°–ê \"–ù–ê–ü–ò–¢–û–ß–ù–ò–ö\" !!!\r\n        const appsScriptUrl = 'https://script.google.com/macros/s/AKfycbwKNmX73aTyRTJ8ynz1voJkMJEz09DobCb3Rald7tU802CjUOQ1ZybJMfMigMqPzqJY/exec'; \r\n\r\n        // --- –û—Ç–ø—Ä–∞–≤–∫–∞ –¥–∞–Ω–Ω—ã—Ö ---\r\n        const response = await fetch(appsScriptUrl, {\r\n          method: \"POST\",\r\n          body: formDataToSend \r\n        });\r\n\r\n        const result = await response.json(); \r\n\r\n        if (response.ok && result.status === 'success') {\r\n          Swal.fire({\r\n            title: \"–û–ø—Ä–æ—Å - –ù–∞–ø–∏—Ç–æ—á–Ω–∏–∫\", \r\n            html: `<b>–°–æ—Ç—Ä—É–¥–Ω–∏–∫:</b> ${employeeName}<br><b>–†–µ–∑—É–ª—å—Ç–∞—Ç:</b> ${percent}%<br><b>–ö–æ–º–º–µ–Ω—Ç–∞—Ä–∏–π:</b><br>${comment}`,\r\n            icon: 'success',\r\n            confirmButtonText: 'OK',\r\n            confirmButtonColor: '#d32f2f'\r\n          });\r\n          form.reset(); \r\n        } else {\r\n          console.error(\"–û—à–∏–±–∫–∞ –ø—Ä–∏ –æ—Ç–ø—Ä–∞–≤–∫–µ –≤ Apps Script:\", result);\r\n          Swal.fire({\r\n            title: \"–û—à–∏–±–∫–∞\",\r\n            text: `–ù–µ —É–¥–∞–ª–æ—Å—å –æ—Ç–ø—Ä–∞–≤–∏—Ç—å –¥–∞–Ω–Ω—ã–µ: ${result.message || '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–∞—è –æ—à–∏–±–∫–∞.'}`,\r\n            icon: 'error',\r\n            confirmButtonText: 'OK',\r\n            confirmButtonColor: '#d32f2f'\r\n          });\r\n        }\r\n      } catch (error) {\r\n        console.error(\"–û–±—â–∞—è –æ—à–∏–±–∫–∞:\", error);\r\n        Swal.fire({\r\n          title: \"–û—à–∏–±–∫–∞\",\r\n          text: \"–ü—Ä–æ–∏–∑–æ—à–ª–∞ –Ω–µ–ø—Ä–µ–¥–≤–∏–¥–µ–Ω–Ω–∞—è –æ—à–∏–±–∫–∞. –ü—Ä–æ–≤–µ—Ä—å—Ç–µ –∫–æ–Ω—Å–æ–ª—å.\",\r\n          icon: 'error',\r\n          confirmButtonText: 'OK',\r\n          confirmButtonColor: '#d32f2f'\r\n        });\r\n      } finally {\r\n        btn.disabled = false;\r\n        btn.textContent = \"–ó–∞–∫–æ–Ω—á–∏—Ç—å –æ–ø—Ä–æ—Å\";\r\n      }\r\n    });\r\n  </script>\r\n</body>\r\n</html>\r\n",
