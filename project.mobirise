          "_customHTML": "<!DOCTYPE html>\r\n<html lang=\"ru\">\r\n<head>\r\n  <meta charset=\"UTF-8\">\r\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n  <title>–û–±—â–∏–π –æ–ø—Ä–æ—Å</title>\r\n  \r\n  <!-- –°—Ç–∏–ª–∏ -->\r\n  <style>\r\n    /* –ì–ª–æ–±–∞–ª—å–Ω—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –¥–ª—è –æ—Ç–∑—ã–≤—á–∏–≤–æ–≥–æ –¥–∏–∑–∞–π–Ω–∞ */\r\n    *, *::before, *::after {\r\n      box-sizing: border-box;\r\n    }\r\n\r\n    body {\r\n      background-color: #f8f8f8;\r\n      display: flex;\r\n      flex-direction: column; /* –≠–ª–µ–º–µ–Ω—Ç—ã –≤ —Å—Ç–æ–ª–±–∏–∫, –¥–ª—è —Ü–µ–Ω—Ç—Ä–∏—Ä–æ–≤–∞–Ω–∏—è */\r\n      justify-content: center;\r\n      align-items: center;\r\n      min-height: 100vh;\r\n      margin: 0;\r\n      padding: 20px; /* –û—Ç—Å—Ç—É–ø –ø–æ –∫—Ä–∞—è–º –¥–ª—è –∞–¥–∞–ø—Ç–∏–≤–Ω–æ—Å—Ç–∏ */\r\n      font-family: sans-serif;\r\n    }\r\n    .form-container {\r\n      max-width: 600px;\r\n      width: 100%; /* –ó–∞–Ω–∏–º–∞–µ—Ç –≤—Å—é –¥–æ—Å—Ç—É–ø–Ω—É—é —à–∏—Ä–∏–Ω—É –¥–æ max-width */\r\n      padding: 30px;\r\n      border: 1px solid #ddd;\r\n      border-radius: 12px;\r\n      background-color: #FFE4E1; /* –°–≤–µ—Ç–ª–æ-—Ä–æ–∑–æ–≤—ã–π */\r\n      box-shadow: 0 8px 20px rgba(0,0,0,0.1);\r\n    }\r\n    .avatar-img {\r\n      width: 150px;\r\n      height: 150px;\r\n      border-radius: 50%;\r\n      object-fit: cover;\r\n      margin: 0 auto 20px auto; /* –¶–µ–Ω—Ç—Ä–∏—Ä–æ–≤–∞–Ω–∏–µ */\r\n      display: block;\r\n      border: 3px solid #d32f2f; /* –¶–≤–µ—Ç –∫–∞–∫ —É –∫–Ω–æ–ø–∫–∏ */\r\n      box-shadow: 0 0 15px rgba(0,0,0,0.15);\r\n    }\r\n    .form-title {\r\n      font-size: 28px;\r\n      font-weight: bold;\r\n      text-align: center;\r\n      margin-bottom: 30px;\r\n      color: #333;\r\n    }\r\n    .question-frame { /* –†–∞–º–∫–∞ –¥–ª—è –≤–æ–ø—Ä–æ—Å–æ–≤ –∏ –ø–æ–ª–µ–π –≤–≤–æ–¥–∞ */\r\n      border: 1px solid #ffcccb; \r\n      border-radius: 8px;\r\n      padding: 15px;\r\n      margin-bottom: 25px; \r\n      background-color: #fffafa; \r\n      box-shadow: 0 2px 5px rgba(0,0,0,0.05);\r\n    }\r\n    .question-frame label { /* –°—Ç–∏–ª—å –¥–ª—è —Ç–µ–∫—Å—Ç–∞ –≤–æ–ø—Ä–æ—Å–∞/–ø–æ–ª—è –≤–Ω—É—Ç—Ä–∏ —Ä–∞–º–∫–∏ */\r\n        margin-top: 0 !important; \r\n        margin-bottom: 12px !important; \r\n        display: block !important; \r\n        font-weight: bold !important; \r\n        color: #555 !important; \r\n        line-height: 1.4 !important; \r\n        word-wrap: break-word; \r\n        overflow-wrap: break-word; \r\n    }\r\n    label { /* –°–±—Ä–æ—Å –æ–±—â–∏—Ö —Å—Ç–∏–ª–µ–π label, —á—Ç–æ–±—ã –æ–Ω–∏ –Ω–µ –≤–ª–∏—è–ª–∏ –Ω–∞ —ç–ª–µ–º–µ–Ω—Ç—ã –≤–Ω—É—Ç—Ä–∏ —Ä–∞–º–æ–∫ */\r\n        font-weight: normal; \r\n        display: inline; \r\n        margin: 0; \r\n        padding: 0; \r\n        color: inherit;\r\n        line-height: normal;\r\n    }\r\n\r\n    input[type=\"text\"], textarea {\r\n      width: 100%; \r\n      padding: 12px;\r\n      border-radius: 8px;\r\n      border: 1px solid #ccc;\r\n      font-size: 16px;\r\n      margin-bottom: 0; \r\n    }\r\n    textarea {\r\n      resize: vertical; \r\n      min-height: 80px;\r\n    }\r\n    .radio-group {\r\n      display: flex; \r\n      flex-direction: row; /* –í–∞—Ä–∏–∞–Ω—Ç—ã –≤ —Ä—è–¥ */\r\n      gap: 20px; \r\n      margin-bottom: 0; \r\n      flex-wrap: wrap; /* –ü–µ—Ä–µ–Ω–æ—Å –Ω–∞ –Ω–æ–≤—É—é —Å—Ç—Ä–æ–∫—É, –µ—Å–ª–∏ –Ω–µ —Ö–≤–∞—Ç–∞–µ—Ç –º–µ—Å—Ç–∞ */\r\n      justify-content: flex-start; \r\n    }\r\n    .radio-option {\r\n      display: flex;\r\n      align-items: center;\r\n      gap: 12px;\r\n      cursor: pointer;\r\n      padding: 10px;\r\n      border-radius: 8px;\r\n      background-color: transparent; \r\n      transition: background-color 0.2s ease;\r\n      flex-shrink: 0; \r\n    }\r\n    .radio-option:hover {\r\n      background-color: #f0f0f0; \r\n    }\r\n    .radio-option input[type=\"radio\"] {\r\n      display: none; \r\n    }\r\n    .custom-radio {\r\n      flex-shrink: 0; \r\n      width: 35px; \r\n      height: 35px;\r\n      background-color: #eee;\r\n      border-radius: 50%;\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: center;\r\n      font-weight: bold;\r\n      color: #333;\r\n      border: 2px solid transparent;\r\n      transition: 0.2s;\r\n      font-size: 18px;\r\n    }\r\n    /* –¶–≤–µ—Ç –∞–∫—Ç–∏–≤–Ω–æ–π –∫–Ω–æ–ø–∫–∏ –¥–ª—è –û–±—â–µ–≥–æ –æ–ø—Ä–æ—Å–∞ */\r\n    .radio-option input[type=\"radio\"]:checked + .custom-radio {\r\n      border-color: #d32f2f; /* –ö—Ä–∞—Å–Ω—ã–π, –∫–∞–∫ —É –∫–Ω–æ–ø–∫–∏ submit */\r\n      background-color: #d32f2f; /* –ö—Ä–∞—Å–Ω—ã–π */\r\n      color: white;\r\n      box-shadow: 0 0 10px rgba(211, 47, 47, 0.5); /* –°–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤—É—é—â–∞—è —Ç–µ–Ω—å */\r\n    }\r\n    .submit-btn {\r\n      margin-top: 40px;\r\n      padding: 15px 25px;\r\n      background-color: #d32f2f;\r\n      color: white;\r\n      font-size: 18px;\r\n      font-weight: bold;\r\n      border: none;\r\n      border-radius: 10px;\r\n      cursor: pointer;\r\n      width: 100%;\r\n      transition: background-color 0.3s ease, transform 0.2s ease;\r\n      box-shadow: 0 4px 10px rgba(0,0,0,0.3);\r\n    }\r\n    .submit-btn:hover:not(:disabled) {\r\n      background-color: #b71c1c;\r\n      transform: translateY(-2px);\r\n    }\r\n    .submit-btn:disabled {\r\n      background-color: #ff7f7f; \r\n      cursor: not-allowed;\r\n      box-shadow: none;\r\n    }\r\n    /* –°—Ç–∏–ª—å –¥–ª—è —Å–µ–∫—Ü–∏–æ–Ω–Ω—ã—Ö –∑–∞–≥–æ–ª–æ–≤–∫–æ–≤ */\r\n    .section-title {\r\n        background: #d32f2f; /* –ö—Ä–∞—Å–Ω—ã–π —Ñ–æ–Ω */\r\n        color: white;\r\n        padding: 10px;\r\n        margin-top: 30px;\r\n        margin-bottom: 20px; /* –î–æ–±–∞–≤–∏–ª –æ—Ç—Å—Ç—É–ø —Å–Ω–∏–∑—É */\r\n        border-radius: 8px;\r\n        font-weight: bold;\r\n        text-align: center;\r\n    }\r\n\r\n    /* –ú–µ–¥–∏–∞-–∑–∞–ø—Ä–æ—Å—ã –¥–ª—è –∞–¥–∞–ø—Ç–∞—Ü–∏–∏ –ø–æ–¥ –º–æ–±–∏–ª—å–Ω—ã–µ */\r\n    @media (max-width: 768px) {\r\n        body { padding: 15px; }\r\n        .form-container { padding: 20px; }\r\n        .form-title { font-size: 24px; margin-bottom: 20px; }\r\n        .question-frame { padding: 10px; margin-bottom: 20px; }\r\n        .question-frame label { font-size: 15px; }\r\n        input[type=\"text\"], textarea { \r\n            font-size: 15px;\r\n            padding: 10px;\r\n        }\r\n        .radio-group {\r\n            flex-direction: column; /* –í–∞—Ä–∏–∞–Ω—Ç—ã –≤ —Å—Ç–æ–ª–±–∏–∫ –Ω–∞ –º–æ–±–∏–ª—å–Ω—ã—Ö */\r\n            gap: 8px; \r\n        }\r\n        .radio-option {\r\n            padding: 8px;\r\n        }\r\n        .custom-radio {\r\n            width: 30px; \r\n            height: 30px;\r\n            font-size: 16px;\r\n        }\r\n        .submit-btn {\r\n            font-size: 16px;\r\n            padding: 12px 20px;\r\n            margin-top: 30px;\r\n        }\r\n    }\r\n\r\n    @media (max-width: 480px) { /* –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –∞–¥–∞–ø—Ç–∞—Ü–∏—è –¥–ª—è –æ—á–µ–Ω—å –º–∞–ª–µ–Ω—å–∫–∏—Ö —ç–∫—Ä–∞–Ω–æ–≤ */\r\n        body { padding: 10px; }\r\n        .form-container { padding: 15px; }\r\n        .form-title { font-size: 22px; margin-bottom: 15px; }\r\n        .avatar-img { width: 120px; height: 120px; }\r\n        .question-frame { margin-bottom: 15px; }\r\n    }\r\n  </style>\r\n  \r\n  <!-- SweetAlert2 –¥–ª—è –∫—Ä–∞—Å–∏–≤—ã—Ö –≤—Å–ø–ª—ã–≤–∞—é—â–∏—Ö –æ–∫–æ–Ω -->\r\n  <script src=\"https://cdn.jsdelivr.net/npm/sweetalert2@11\"></script>\r\n</head>\r\n<body>\r\n  \r\n  <div class=\"form-container\">\r\n    <img src=\"https://i.imgur.com/SxzkAWe.jpeg\" alt=\"–ê–≤–∞—Ç–∞—Ä\" class=\"avatar-img\">\r\n    \r\n    <div class=\"form-title\">–û–±—â–∏–π –æ–ø—Ä–æ—Å</div>\r\n\r\n    <form id=\"customForm\">\r\n      <!-- –°–∫—Ä—ã—Ç–æ–µ –ø–æ–ª–µ –¥–ª—è –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ü–∏–∏ –æ–ø—Ä–æ—Å–∞ -->\r\n      <input type=\"hidden\" name=\"surveyId\" value=\"general_survey\"> \r\n\r\n      <!-- –ò–º—è HR -->\r\n      <div class=\"question-frame\">\r\n        <label for=\"yourName\">–í–∞—à–µ –∏–º—è:</label>\r\n        <input type=\"text\" id=\"yourName\" name=\"yourName\" required>\r\n      </div>\r\n      \r\n      <!-- –ò–º—è —Å–æ—Ç—Ä—É–¥–Ω–∏–∫–∞ -->\r\n      <div class=\"question-frame\">\r\n        <label for=\"employeeName\">–ò–º—è —Å–æ—Ç—Ä—É–¥–Ω–∏–∫–∞:</label>\r\n        <input type=\"text\" id=\"employeeName\" name=\"employeeName\" required>\r\n      </div>\r\n\r\n      <div class=\"section-title\">–ú—ã—Ç—å–µ, –¥–µ–∑–∏–Ω—Ñ–µ–∫—Ü–∏—è —Ä—É–∫ –∏ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ –∫—Ä–µ–º–æ–≤</div>\r\n      <!-- –í–æ–ø—Ä–æ—Å 1 -->\r\n      <div class=\"question-frame\">\r\n        <label>1. –ù–∞–º–æ—á–∏—Ç—å —Ä—É–∫–∏ –∏ –ø—Ä–µ–¥–ø–ª–µ—á—å—è –≤–æ–¥–æ–π –≤ —Ä–∞–∫–æ–≤–∏–Ω–µ –¥–ª—è –º—ã—Ç—å—è —Ä—É–∫ (–¢ –≤–æ–¥—ã 38 –° - 42 –°):</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q1_wet_hands_temp\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q1_wet_hands_temp\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 2 -->\r\n      <div class=\"question-frame\">\r\n        <label>2. –ù–∞–Ω–µ—Å—Ç–∏ –∞–Ω—Ç–∏–±–∞–∫—Ç–µ—Ä–∏–∞–ª—å–Ω–æ–µ –º—ã–ª–æ –†-506, –Ω–∞–º—ã–ª–∏—Ç—å —Ä—É–∫–∏ –∏ –ø—Ä–µ–¥–ø–ª–µ—á—å—è:</label>\r\n        <div class=\"radio-group\">\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q2_apply_soap\" value=\"–ó–Ω–∞–µ—Ç\" required><div class=\"custom-radio\">A</div> –ó–Ω–∞–µ—Ç</label>\r\n          <label class=\"radio-option\"><input type=\"radio\" name=\"q2_apply_soap\" value=\"–ù–µ –∑–Ω–∞–µ—Ç\"><div class=\"custom-radio\">B</div> –ù–µ –∑–Ω–∞–µ—Ç</label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- –í–æ–ø—Ä–æ—Å 3 -->\r\n      <div class=\"question-frame\">\r\n        <label>3. –¢—â–∞—Ç–µ–ª—å–Ω–æ –≤—ã–º—ã—Ç—å —Ä—É–∫–∏ –≤ —Ç–µ—á–µ–Ω–∏–µ 20 —Å–µ–∫—É–Ω–¥, –≤–∫–ª—é—á–∞—è –≤—Å–µ –∑–