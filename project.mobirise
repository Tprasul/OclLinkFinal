          "_customHTML": "<!DOCTYPE html>\r\n<html lang=\"ru\">\r\n<head>\r\n  <meta charset=\"UTF-8\">\r\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n  <title>HR - –û–ø—Ä–æ—Å</title>\r\n  \r\n  <!-- –°—Ç–∏–ª–∏ -->\r\n  <style>\r\n    /* –ì–ª–æ–±–∞–ª—å–Ω—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –¥–ª—è –æ—Ç–∑—ã–≤—á–∏–≤–æ–≥–æ –¥–∏–∑–∞–π–Ω–∞ */\r\n    *, *::before, *::after {\r\n      box-sizing: border-box;\r\n    }\r\n\r\n    body {\r\n      background-color: #f8f8f8;\r\n      display: flex;\r\n      flex-direction: column; /* –ò–∑–º–µ–Ω–µ–Ω–æ –Ω–∞ column, —á—Ç–æ–±—ã –∫–æ–Ω—Ç–µ–Ω—Ç –º–æ–≥ –≤—ã—Ä–∞–≤–Ω–∏–≤–∞—Ç—å—Å—è –ø–æ —Ü–µ–Ω—Ç—Ä—É –≤–µ—Ä—Ç–∏–∫–∞–ª—å–Ω–æ */\r\n      justify-content: center;\r\n      align-items: center;\r\n      min-height: 100vh;\r\n      margin: 0;\r\n      padding: 20px; /* –û—Ç—Å—Ç—É–ø –ø–æ –∫—Ä–∞—è–º –¥–ª—è –º–æ–±–∏–ª—å–Ω—ã—Ö */\r\n      font-family: sans-serif;\r\n    }\r\n    .form-container {\r\n      max-width: 600px;\r\n      width: 100%; /* –ó–∞–Ω–∏–º–∞–µ—Ç –≤—Å—é –¥–æ—Å—Ç—É–ø–Ω—É—é —à–∏—Ä–∏–Ω—É –¥–æ max-width */\r\n      padding: 30px;\r\n      border: 1px solid #ddd;\r\n      border-radius: 12px;\r\n      background-color: #FFE4E1; /* Light red/pink */\r\n      box-shadow: 0 8px 20px rgba(0,0,0,0.1);\r\n      /* margin: auto; // –£–±–∏—Ä–∞–µ–º, —Ç.–∫. flexbox body –±—É–¥–µ—Ç —Ü–µ–Ω—Ç—Ä–∏—Ä–æ–≤–∞—Ç—å */\r\n    }\r\n    .avatar-img {\r\n      width: 150px;\r\n      height: 150px;\r\n      border-radius: 50%;\r\n      object-fit: cover;\r\n      margin: 0 auto 20px auto; /* Centered */\r\n      display: block;\r\n      border: 3px solid #d32f2f; /* Matching button color */\r\n      box-shadow: 0 0 15px rgba(0,0,0,0.15);\r\n    }\r\n    .form-title {\r\n      font-size: 28px;\r\n      font-weight: bold;\r\n      text-align: center;\r\n      margin-bottom: 30px;\r\n      color: #333;\r\n    }\r\n    .question-frame { /* Style for question frames */\r\n      border: 1px solid #ffcccb; /* Lighter border for frame */\r\n      border-radius: 8px;\r\n      padding: 15px;\r\n      margin-bottom: 25px; /* Space below each frame */\r\n      background-color: #fffafa; /* Very light background for frame */\r\n      box-shadow: 0 2px 5px rgba(0,0,0,0.05);\r\n    }\r\n    .question-frame label { /* Adjust label inside frame */\r\n        margin-top: 0; \r\n        margin-bottom: 12px; \r\n        display: block; \r\n        font-weight: bold; \r\n        color: #555; \r\n        line-height: 1.4; \r\n        word-wrap: break-word; /* –û–±–µ—Å–ø–µ—á–∏–≤–∞–µ—Ç –ø–µ—Ä–µ–Ω–æ—Å –¥–ª–∏–Ω–Ω—ã—Ö —Å–ª–æ–≤ */\r\n        overflow-wrap: break-word; /* –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –ø–æ–¥–¥–µ—Ä–∂–∫–∞ –ø–µ—Ä–µ–Ω–æ—Å–∞ */\r\n    }\r\n    /* –°–±—Ä–æ—Å —Å—Ç–∏–ª–µ–π –¥–ª—è –æ—Å–Ω–æ–≤–Ω–æ–≥–æ label, —Ç–∞–∫ –∫–∞–∫ –æ–Ω–∏ —Ç–µ–ø–µ—Ä—å –≤ .question-frame label */\r\n    label {\r\n        font-weight: normal; \r\n        display: inline; \r\n        margin-top: 0;\r\n        margin-bottom: 0;\r\n        color: inherit;\r\n        line-height: normal;\r\n    }\r\n\r\n    input[type=\"text\"], textarea {\r\n      width: 100%; \r\n      padding: 12px;\r\n      border-radius: 8px;\r\n      border: 1px solid #ccc;\r\n      font-size: 16px;\r\n      margin-bottom: 0; \r\n    }\r\n    textarea {\r\n      resize: vertical; \r\n      min-height: 80px;\r\n    }\r\n    .radio-group {\r\n      display: flex; \r\n      flex-direction: row; /* Default: options on one line */\r\n      gap: 20px; \r\n      margin-bottom: 0; \r\n      flex-wrap: wrap; /* Allow wrapping on smaller screens */\r\n      justify-content: flex-start; /* Align items to the start */\r\n    }\r\n    .radio-option {\r\n      display: flex;\r\n      align-items: center;\r\n      gap: 12px;\r\n      cursor: pointer;\r\n      padding: 10px;\r\n      border-radius: 8px;\r\n      background-color: transparent; \r\n      transition: background-color 0.2s ease;\r\n      flex-shrink: 0; \r\n    }\r\n    .radio-option:hover {\r\n      background-color: #f0f0f0; \r\n    }\r\n    .radio-option input[type=\"radio\"] {\r\n      display: none;\r\n    }\r\n    .custom-radio {\r\n      flex-shrink: 0; \r\n      width: 35px; \r\n      height: 35px;\r\n      background-color: #eee;\r\n      border-radius: 50%;\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: center;\r\n      font-weight: bold;\r\n      color: #333;\r\n      border: 2px solid transparent;\r\n      transition: 0.2s;\r\n      font-size: 18px;\r\n    }\r\n    .radio-option input[type=\"radio\"]:checked + .custom-radio {\r\n      border-color: #d32f2f;\r\n      background-color: #d32f2f;\r\n      color: white;\r\n      box-shadow: 0 0 10px rgba(211, 47, 47, 0.5);\r\n    }\r\n    .submit-btn {\r\n      margin-top: 40px;\r\n      padding: 15px 25px;\r\n      background-color: #d32f2f;\r\n      color: white;\r\n      font-size: 18px;\r\n      font-weight: bold;\r\n      border: none;\r\n      border-radius: 10px;\r\n      cursor: pointer;\r\n      width: 100%;\r\n      transition: background-color 0.3s ease, transform 0.2s ease;\r\n      box-shadow: 0 4px 10px rgba(0,0,0,0.3);\r\n    }\r\n    .submit-btn:hover:not(:disabled) {\r\n      background-color: #b71c1c;\r\n      transform: translateY(-2px);\r\n    }\r\n    .submit-btn:disabled {\r\n      background-color: #ff7f7f; \r\n      cursor: not-allowed;\r\n      box-shadow: none;\r\n    }\r\n\r\n    /* –ú–µ–¥–∏–∞-–∑–∞–ø—Ä–æ—Å—ã –¥–ª—è –∞–¥–∞–ø—Ç–∞—Ü–∏–∏ –Ω–∞ –º–æ–±–∏–ª—å–Ω—ã—Ö —É—Å—Ç—Ä–æ–π—Å—Ç–≤–∞—Ö */\r\n    @media (max-width: 768px) {\r\n        body {\r\n            padding: 15px; /* –ú–µ–Ω—å—à–∏–π –æ—Ç—Å—Ç—É–ø –Ω–∞ –º–∞–ª–µ–Ω—å–∫–∏—Ö —ç–∫—Ä–∞–Ω–∞—Ö */\r\n        }\r\n        .form-container {\r\n            padding: 20px;\r\n        }\r\n        .form-title {\r\n            font-size: 24px;\r\n            margin-bottom: 20px;\r\n        }\r\n        .question-frame {\r\n            padding: 10px;\r\n            margin-bottom: 20px;\r\n        }\r\n        .question-frame label {\r\n            fo